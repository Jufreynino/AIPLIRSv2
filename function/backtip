function add_poultry_disease()
{
    if(isset($_POST['btn_add_poultry']))
    {
        global $con;
        date_default_timezone_set('Asia/Manila');
        $today = date("m/d/Y h:i:sa");
        $date = date("m/d/Y");
        $time = date("h:i:sa");
        $disease_code = mysqli_real_escape_string($con, $_POST['disease_code']);
        $disease_description = mysqli_real_escape_string($con, $_POST['disease_description']);
        $disease_level = mysqli_real_escape_string($con, $_POST['disease_level']);
        $disease_type = mysqli_real_escape_string($con, $_POST['disease_type']);
        $disease_category = mysqli_real_escape_string($con, $_POST['disease_category']);


        $sql = mysqli_query($con, "INSERT INTO disease_tbl (disease_code,disease_description,disease_level,disease_status,disease_type,disease_category,disease_date,disease_time)
                                                                VALUES('$disease_code','$disease_description','$disease_level','Active','$disease_type','$disease_category','$date','$time')");

        ?>
        <script type="text/javascript">
            window.location = "poultry.php";
        </script>
        <?php

    }
}



function add_antemortem_pdp_disease()
{
    if(isset($_POST['btn_add_pdp']))
    {
        global $con;
        date_default_timezone_set('Asia/Manila');
        $today = date("m/d/Y h:i:sa");
        $date = date("m/d/Y");
        $time = date("h:i:sa");
        $disease_code = mysqli_real_escape_string($con, $_POST['disease_code']);
        $disease_description = mysqli_real_escape_string($con, $_POST['disease_description']);
        $disease_kind_of_species = mysqli_real_escape_string($con, $_POST['disease_kind_of_species']);
        $notifiable = mysqli_real_escape_string($con, $_POST['notifiable']);
        $disease_type = 'PDP';
        $disease_category = 'Antemortem';


        $sql = mysqli_query($con, "INSERT INTO disease_tbl (disease_code,disease_description,disease_level,disease_status,disease_type,disease_category,disease_date,disease_time,disease_notifiable,disease_species)
                                                                VALUES('$disease_code','$disease_description','Critical','Active','$disease_type','$disease_category','$date','$time','$notifiable','$disease_kind_of_species')");

        ?>
        <script type="text/javascript">
            window.location = "antemortem_pdp.php";
        </script>
        <?php
    }
}



