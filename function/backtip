function fetch_all_critical_antemortem_suspected()
{
    global $con;
         date_default_timezone_set('Asia/Manila');
        $today = date("m/d/Y h:i:sa");
        $date = date("m/d/Y");
        $time = date("h:i:sa");
        $sql    =    "SELECT DISTINCT(am_table.s_cause) as s_cause FROM disease_report_tbl INNER JOIN am_table ON disease_report_tbl.drr_id = am_table.am_table AND disease_report_tbl.dr_date='$date'";
        $result = mysqli_query($con, $sql); 
        while($row = mysqli_fetch_assoc($result)){


                 $sqls    =    "SELECT SUM(am_table.s_heads) as s_heads,SUM(am_table.s_weight) as s_weights, disease_tbl.disease_description as disease_description FROM disease_tbl LEFT JOIN am_table ON disease_tbl.d_id = am_table.s_cause WHERE disease_level='Critical' AND am_table.s_cause='".$row['s_cause']."' GROUP BY disease_tbl.disease_description";
                    $results = mysqli_query($con, $sqls); 
                    while($rows = mysqli_fetch_assoc($results)){

                    ?>
                    <tr>
                        <td ><?php echo $rows['disease_description'] ?></td>
                        <td class="text-right"><?php echo $rows['s_heads'] ?></td>
                        <td class="text-right"><?php echo $rows['s_weights'] ?></td>
                    </tr>
                    <?php
         }
    }
}

