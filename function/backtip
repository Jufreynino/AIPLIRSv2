                        ?>
                        <td style="background-color: white !important;"  class="text-right"><?php echo number_format($chicken_row['chicken_fit_number_of_head']); ?></td>
                        <td style="background-color: #bdbaba  !important;"  class="text-right"><?php echo number_format($chicken_row['chicken_fit_weight']); ?></td>
                        <?php } ?>
                </tr>

                <?php
                }
                }
        else{
            ?>
                <td colspan="17" class="text-center" style="color: white;">NO DATA</td>
            <?php 
        }

}


function mmir_posms_region(){
    global $con;
    $region = $_SESSION['region'];

    $from = $_GET['from'];
    $fromdate = DateTime::createFromFormat('Y-m-d',$from);
    $from_date = $fromdate->format('m/d/Y');


    $to = $_GET['to'];
    $todate = DateTime::createFromFormat('Y-m-d',$to);
    $to_date = $todate->format('m/d/Y');

    $province = $_GET['province'];

    $sql = "SELECT * FROM fit_human_consumption WHERE fit_region='$region'  AND fit_date BETWEEN '$from_date' AND '$to_date' GROUP BY fit_province";
    $result = $con->query($sql);

    if(mysqli_num_rows($result) > 0)
    {
            while($row = $result->fetch_assoc())
            {
                ?>
