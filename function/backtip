    $sql = "SELECT * FROM meat_establishment_tbl WHERE  me_id='$id'  ";
    $result = $con->query($sql);
    $row = $result->fetch_assoc();
    echo $row['me_plant_name'];
}




function show_owner_mio_posmd_drr()
{
 $date = date("m/d/Y");
    $id = $_GET['mid'];
    global $con;
    $sql = "SELECT DISTINCT(drr_date),meat_establishment_tbl.me_plant_name as me_plant_name,meat_establishment_tbl.me_type as me_type,meat_establishment_tbl.me_lto_number as me_lto_number,meat_establishment_tbl.me_classification as me_classification,meat_establishment_tbl.me_expiry as me_expiry  FROM meat_establishment_tbl INNER JOIN ddr_table on meat_establishment_tbl.me_id= ddr_table.me_id WHERE meat_establishment_tbl.me_id='$id' AND ddr_table.drr_status='1' ORDER BY ddr_table.drr_date DESC";
    $result = $con->query($sql);

        while($row = $result->fetch_assoc()) {
            ?>
            
            <?php if($row['drr_date'] == $date) { ?>
            <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8; background-color:#d3d4d7;">
            <?php } else{  ?>
                <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8;">
            <?php } ?>

                <td><b> <?php echo $row['drr_date']  ?></b></td>
                <td><b> <?php echo $row['me_plant_name']  ?></b></td>
                <td><b> <?php echo $row['me_type']  ?></b></td>
                <td><b> <?php echo $row['me_classification']  ?></b></td>
                <td><b> <?php echo $row['me_lto_number']  ?></b></td>
                <td><b> <?php echo $row['me_expiry']  ?></b></td>
           
                <td  style="width:110px;">
                    <a href="dmir.php?md=<?php echo $id ?>&mdate=<?php echo $row['drr_date'] ?>  " class="btn btn-primary btn-icon">View DRR</a>
                    <a href="dmir.php?md=<?php echo $id ?>&mdate=<?php echo $row['drr_date'] ?>  " class="btn btn-info btn-icon">View DMIR</a>
                </td>
            </tr>
            <?php
        }
}


function show_mio_drr()
{
    $date = date("m/d/Y");
    $id = $_SESSION['employee_id'];
    global $con;
    $sql = "SELECT * FROM meat_establishment_tbl WHERE employee_id='$id'";
    $result = $con->query($sql);

        
    if ($result->num_rows > 0) {
        while($row = $result->fetch_assoc()) {
            ?>
            <tr style="border: 1px solid black;">
                <td> <?php echo $row['me_plant_name']  ?></td>
                <td> <?php echo $row['me_type']  ?></td>
                <td> <?php echo $row['me_classification']  ?></td>
                <td> <?php echo $row['me_lto_number']  ?></td>
                <td> <?php echo $row['me_expiry']  ?></td>
           
                <td style="width:100px;">
                    <a href="drr.php?md=<?php echo $row['me_id'] ?>  " class="btn btn-info btn-icon">Select Meat Establishment</a>
                </td>
            </tr>
            <?php
        }
    } else {
        ?>
        <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8;">
            <td class="text-center" colspan="6"><b> No Records Found!</b></td>
        </tr>
        <?php
    }
}


