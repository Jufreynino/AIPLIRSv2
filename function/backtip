    $date = $_GET['date'];
    global $con;
    $sql = "SELECT * FROM pm_table INNER JOIN ddr_table ON pm_table.drr_id = ddr_table.drr_id WHERE carcass_cause='$d_id' OR lung_cause='$d_id' OR liver_cause='$d_id' OR heart_cause='$d_id' OR intestine_cause='$d_id' OR trimming_cause='$d_id' OR feet_cause='$d_id' OR kidney_cause='$d_id' OR spleen_cause='$d_id' ";
    $result = $con->query($sql);
    while($row = $result->fetch_assoc()) {



                ?>
                                    <tr>
                                        <td style="width:150px;"><?php echo $row['drr_meat_dealer'] ?></td>
