function show_md_drr_mio()
{
    date_default_timezone_set('Asia/Manila');
    $today = date("m/d/Y h:i:sa");
    $date = date("m/d/Y");
    $time = date("h:i:sa");
    $date_md = $_GET['mdate'];
    $mid = $_GET['md'];

    global $con;

    $sql = "SELECT * FROM ddr_table WHERE drr_date='$date_md' AND me_id='$mid' ORDER BY drr_time DESC ";
    $result = $con->query($sql);



    if ($result->num_rows > 0) {
        
    while($row = $result->fetch_assoc()) {
     
        
        ?>
        <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8;">
            <td><b> <?php echo $row['drr_time'];  ?></b></td>
            <td><b> <?php echo $row['drr_meat_dealer']  ?></b></td>
            <td><b> <?php echo $row['drr_species']  ?></b></td>
            <td> <?php echo $row['drr_province_code'].', '.$row['drr_city_code'].', '.$row['drr_barangay_code'].', '.$row['drr_farm_origin']  ?></td>
            <td>
                <?php if($row['drr_status'] == 0) {?>
                    <span class="badge bg-warning">Pending</span>
                <?php } else { ?>
                <span class="badge bg-success">Inspected</span>

                <?php } ?>

            </td>
            <td style="width:170px;">
                
                <?php if($row['drr_date'] == $date ){ ?>
                <?php  if($row['drr_status'] != 0) {?>
                <a href="#" class="btn-sm btn bg-teal " disable style="opacity:0.6;">Conduct Inspection</button>
              
                <?php } else {?>
                <a href="antemortem.php?med=<?php echo $row['me_id'] ?>&mid=<?php echo $row['drr_id'] ?>&mdate=<?php echo $row['drr_date'] ?>&species=<?php echo $row['drr_species'] ?>&category=<?php echo $row['drr_category'] ?>" class=" btn-sm btn bg-teal item-detail" >Conduct Inspection</button>

                <?php } ?>
            </td>
            <?php } else {?>
                <a href="#" class="btn-sm btn bg-teal " disable style="opacity:0.6;">Conduct Inspection</button>


                <?php } ?>

            </td>
        </tr>
        <?php

    }
    } else {
        ?>
        <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8;">
            <td class="text-center" colspan="6"><b> No Records Found!</b></td>
        </tr>
        <?php
    }
}

function validate_conducted_inspection()
{

    $date = date("m/d/Y");
    $date_md = $_GET['mdate'];
    $mid = $_GET['md'];
    global $con;
    $sql = "SELECT * FROM ddr_table WHERE drr_date='$date_md' AND me_id='$mid' ORDER BY drr_time DESC ";
    $result = $con->query($sql);
    if ($result->num_rows > 0) {
        
    while($row = $result->fetch_assoc()) {
     
        
        ?>
        <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8;">
            <td><b> <?php echo $row['drr_time'];  ?></b></td>
            <td><b> <?php echo $row['drr_meat_dealer']  ?></b></td>
            <td>
                <?php if($row['drr_status'] == 0) {?>
                    <span class="badge bg-warning">Pending</span>
                <?php } else { ?>
                <span class="badge bg-success">Inspected</span>

                <?php } ?>

            </td>
            <td>
                <a href="antemortem.php?med=<?php echo $row['me_id'] ?>&mid=<?php echo $row['drr_id'] ?>&mdate=<?php echo $row['drr_date'] ?>" class=" btn-sm btn bg-teal item-detail" >View</button>
            </td>
        </tr>
        <?php

    }
    } else {
        ?>
        <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8;">
            <td class="text-center" colspan="6"><b> No Records Found!</b></td>
        </tr>
        <?php
    }
}



function show_plant_profile()
{
    global $con;

    $drr_id = $_GET['mid'];
    $date = $_GET['mdate'];
    $id = $_GET['med'];

    $sql = "SELECT am_table.am_date as am_date, ddr_table.drr_total_head as mtotal_head, ddr_table.drr_total_weight as drr_total_weight, ddr_table.drr_meat_dealer as drr_meat_dealer,ddr_table.drr_species as drr_species, ddr_table.drr_date as drr_date  FROM ddr_table LEFT JOIN am_table on ddr_table.drr_id=am_table.drr_id WHERE ddr_table.drr_id='$drr_id' AND ddr_table.drr_date='$date'";
    $result = $con->query($sql);
    $row = $result->fetch_assoc();
    ?>
    <div class="card">
            <div class="card-header header-elements-inline" style="background-color:#263238; color:white;">
                <span class="card-title font-weight-semibold">A. Receiving of Food Animals for Slaughter</span>
                <div class="header-elements">
                    <div class="list-icons">
                        <a class="list-icons-item" data-action="collapse"></a>
                    </div>
                </div>
            </div>

            <div class="card-body p-0">
                <div class="nav nav-sidebar my-2">
                    <li class="nav-item">
                        <a class="nav-link" style="color:black;">
                       Establishment name
                            <span class=" font-size-sm font-weight-normal ml-auto"> <?php echo plant_name_mio() ?></span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" style="color:black;">
                            Meat Dealer 
                            <span class=" font-size-sm font-weight-normal ml-auto"><?php echo $row['drr_meat_dealer'] ?></span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" style="color:black;">
                            Date
                            <span class=" font-size-sm font-weight-normal ml-auto"><?php echo $row['drr_date'] ?></span>
                        </a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" style="color:black;">
                            Species
                            <span class=" font-size-sm font-weight-normal ml-auto"><?php echo $row['drr_species'] ?></span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" style="color:black;">
                             Head Count
                            <span class=" font-size-sm font-weight-normal ml-auto"><?php echo number_format($row['mtotal_head']) ?></span>
                        </a>
                    </li>
                     <li class="nav-item">
                        <a class="nav-link" style="color:black;">
                              Total Weight
                            <span class=" font-size-sm font-weight-normal ml-auto"><?php echo number_format($row['drr_total_weight'],2) ?></span>
                        </a>
                    </li>
                </div>
            </div>
        </div>
    <?php
}


function show_pass_for_slaughter_report()
{
    global $con;

    $id = $_GET['mid'];
    $date = $_GET['mdate'];

    $sql = "SELECT ddr_table.drr_meat_dealer as drr_meat_dealer, ddr_table.drr_date as drr_date, fhc_table.fhc_number_of_heads as fhc_number_of_heads, meat_establishment_tbl.me_plant_name as me_plant_name, fhc_table.fhc_weight as fhc_weight, ddr_table.drr_species as drr_species   FROM ddr_table LEFT JOIN fhc_table on ddr_table.drr_id=fhc_table.drr_id INNER JOIN meat_establishment_tbl ON ddr_table.me_id=meat_establishment_tbl.me_id WHERE ddr_table.drr_id='$id' AND ddr_table.drr_date='$date'";
