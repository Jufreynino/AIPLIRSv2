function mmir_posmd_province_me(){
    global $con;
    $region = $_SESSION['region'];

    $from = $_GET['from'];
    $fromdate = DateTime::createFromFormat('Y-m-d',$from);
    $from_date = $fromdate->format('m/d/Y');


    $to = $_GET['to'];
    $todate = DateTime::createFromFormat('Y-m-d',$to);
    $to_date = $todate->format('m/d/Y');

    $province = $_GET['province'];

    $sql = "SELECT * FROM fit_human_consumption INNER JOIN meat_establishment_tbl ON fit_human_consumption.me_id=meat_establishment_tbl.me_id WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND fit_province='$province' GROUP BY me_plant_name ";
    $result = $con->query($sql);
    if(mysqli_num_rows($result) > 0)
    {
        while($row = $result->fetch_assoc())
                {
                    ?>
                    <tr>
                           <td   class="sticky-col first-col" style=" background-color:white !important; color:black;"><?php echo $row['me_plant_name']; ?></td>
                            <td  style="color:black; background-color:white !important;"><?php echo $row['me_type']?></td>
                            <td  style="color:black; background-color:white !important;"><?php echo $row['me_lto_number']; ?></td>

                              <?php 
                            $hog_sql = "SELECT SUM(fit_number_of_head) as hog_fit_number_of_head, SUM(fit_weight) as hog_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Hog'   AND fit_province='$province'";
                            $hog_result = $con->query($hog_sql);
                            while($hog_row = $hog_result->fetch_assoc()){
                            ?>
                            <td  style="background-color:white;" class="text-right"><?php echo number_format($hog_row['hog_fit_number_of_head']); ?></td>
                            <td style="background-color: #bdbaba !important;" class="text-right"><?php echo number_format($hog_row['hog_fit_weight']); ?></td>
                            <?php } ?>


                                 <?php 
                            $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Chicken'  AND fit_province='$province'";
