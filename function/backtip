function mmir_posms_me(){
    global $con;
    $region = $_SESSION['region'];

    $from = $_GET['from'];
    $fromdate = DateTime::createFromFormat('Y-m-d',$from);
    $from_date = $fromdate->format('m/d/Y');


    $to = $_GET['to'];
    $todate = DateTime::createFromFormat('Y-m-d',$to);
    $to_date = $todate->format('m/d/Y');

    $me_id = $_GET['me_id'];

    $sql = "SELECT * FROM fit_human_consumption INNER JOIN meat_establishment_tbl ON fit_human_consumption.me_id=meat_establishment_tbl.me_id WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND meat_establishment_tbl.me_id='$me_id' GROUP BY me_plant_name ";
    $result = $con->query($sql);
    if(mysqli_num_rows($result) > 0)
    {
        while($row = $result->fetch_assoc())
                {
                    ?>
                    <tr>
                           <td   class="sticky-col first-col" style=" background-color:#253137 !important; color:White;"><?php echo $row['me_plant_name']; ?></td>
