function modify_antemortem_suspect()
{
    
    global $con;
    $id     =   $_GET['mid'];
    $date     =   $_GET['mdate'];
    $sql = "SELECT am_table.s_heads as s_heads, am_table.s_weight as s_weight, am_table.s_cause as s_cause, disease_tbl.disease_description as disease_description, am_table.am_table as am_table, ddr_table.drr_id as drr_id FROM ddr_table INNER JOIN am_table on ddr_table.drr_id=am_table.drr_id INNER JOIN disease_tbl ON am_table.s_cause=disease_tbl.d_id  WHERE ddr_table.drr_id='$id' AND ddr_table.drr_date='$date'";
    $result = $con->query($sql);

    if ($result->num_rows > 0) {
        while($row = $result->fetch_assoc()) {
                if($row['s_heads'] == '0' || $row['s_weight'] == '0' || $row['s_cause'] == '0' ){ 
                } 
                else
                    {
                            ?>
                                  <tr style="padding:10px !important; margin:px !important; border:1px solid #bdb8b8;">
                                    <td style="font-size:12px !important; "><b> <?php echo number_format($row['s_heads'])  ?></b></td>
                                    <td style="font-size:12px !important; padding:2px;"><?php echo number_format($row['s_weight'],2)  ?></td>
                                    <td style="font-size:12px !important; padding:2px;"><?php echo $row['disease_description']  ?></td>
                                    <td style="font-size:12px !important; padding:2px;">
                                    <button type="button" class="btn bg-danger btn-sm" data-toggle="modal" data-target="#delete_suspect<?php echo $row['am_table'];?>"><i class="icon-cancel-square"></i></button>
                                    </td>
                                  </tr>
                                                <div id="delete_suspect<?php echo $row['am_table'];  ?>" class="modal fade" tabindex="-1" data-backdrop="static">
                                                    <div class="modal-dialog">
                                                        <div class="modal-content">
                                                            <div class="modal-header bg-primary">
                                                                <h6 class="modal-title">Rejected</h6>
                                                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                                                            </div>
                                                            <div class="modal-body">
                                                            <form method="POST" class="form-horizontal">
                                                                    <input type="hidden"  name="id" class="form-control" value="<?php echo $row['am_table'];  ?>">
                                                                    <input type="hidden"  name="drr_id" class="form-control" value="<?php echo $row['drr_id'];  ?>">


                                                                    <h5 class="text-center">Are you sure you want to delete this condemned data?</h5>
                                                            </div>
                                                            <div class="modal-footer">
                                                                <button type="button" class="btn btn-link" data-dismiss="modal">No</button>
                                                                <button type="submit" class="btn bg-danger" name="btn_delete">Yes</button>
                                                            </div>
                                                        </div>
                                                    </form>
                                                    </div>
                                                </div>
                        <?php
                                if(isset($_POST['btn_delete']))
                                    {
                                        $sid   =   $_POST['id'];
                                        $drr_id   =   $_POST['drr_id'];
                                        

                                        $sql = mysqli_query($con, "DELETE FROM am_table  WHERE am_table='$sid'  ");
                                        $delete_report_sql = mysqli_query($con, "DELETE FROM disease_report_tbl  WHERE drr_id='$sid'  ");
                                        ?>
                                        <script type="text/javascript">
                                            window.location = "antemortem.php?med=<?php echo $_GET['med'] ?>&mid=<?php echo $id  ?>&mdate=<?php echo $date ?>&species=<?php echo $_GET['species'] ?>&category=<?php echo $_GET['category'] ?>";
                                        </script>
                                        <?php
                                    }
                    } 
            }
        } else {
            ?>
            <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8;">
                <td class="text-center" colspan="6"><b> No Records Found!</b></td>
            </tr>
            <?php
        }
}

function modify_postmortem_carcass()
{
    
    
    $id     =   $_GET['mid'];
    $date     =   $_GET['mdate'];
    global $con;
    $sql = "SELECT pm_table.carcass_number_of_heads as carcass_number_of_heads, pm_table.carcass_weight as carcass_weight, disease_tbl.disease_description as disease_description, pm_table.carcass_cause as carcass_cause, pm_table.pm_id as pm_id FROM ddr_table INNER JOIN pm_table on ddr_table.drr_id=pm_table.drr_id INNER JOIN disease_tbl ON pm_table.carcass_cause = disease_tbl.d_id WHERE ddr_table.drr_id='$id' AND ddr_table.drr_date='$date'";
    $result = $con->query($sql);

    if ($result->num_rows > 0) {
        while($row = $result->fetch_assoc()) {


            if($row['carcass_number_of_heads'] == '0' || $row['carcass_weight'] == '0' || $row['carcass_cause'] == '0' ){
            ?>
           
            <?php } else{ ?>
                <tr style="padding:10px !important; margin:px !important; border:1px solid #bdb8b8;">
                    <td style="font-size:12px !important; padding:2px;"><?php echo $row['disease_description']  ?></td>
                    <td style="font-size:12px !important; "><b> <?php echo $row['carcass_number_of_heads']  ?></b></td>
                    <td style="font-size:12px !important; padding:2px;"><?php echo $row['carcass_weight']  ?></td>
                    <td style="font-size:12px !important; padding:2px;">

                    <button type="button" class="btn bg-danger btn-sm" data-toggle="modal" data-target="#delete_suspect<?php echo $row['pm_id'];?>"><i class="icon-cancel-square"></i></button>
                    </td>
                </tr>
                                <div id="delete_suspect<?php echo $row['pm_id'];  ?>" class="modal fade" tabindex="-1" data-backdrop="static">
                                    <div class="modal-dialog">
                                        <div class="modal-content">
                                            <div class="modal-header bg-primary">
                                                <h6 class="modal-title">Rejected</h6>
                                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                                            </div>

                                            <div class="modal-body">
                                            <form method="POST" class="form-horizontal">
                                                    <input type="hidden"  name="id" class="form-control" value="<?php echo $row['pm_id'];  ?>">
                                                    <h5 class="text-center">Are you sure you want to delete this condemned data?</h5>
                                            </div>
                                            <div class="modal-footer">
                                                <button type="button" class="btn btn-link" data-dismiss="modal">No</button>
                                                <button type="submit" class="btn bg-danger" name="btn_delete">Yes</button>
                                            </div>
                                        </div>
                                    </form>
                                    </div>
                                </div>
                             <?php
                        if(isset($_POST['btn_delete']))
                        {
                            $sid   =   $_POST['id'];
                            $sql = mysqli_query($con, "DELETE FROM pm_table  WHERE pm_id='$sid'  ");
                            $delete_report_sql = mysqli_query($con, "DELETE FROM disease_report_tbl  WHERE drr_id='$sid'  ");
                            ?>
                                <script type="text/javascript">
                                    window.location = "postmortem.php?med=<?php echo $_GET['med'] ?>&mid=<?php echo $id  ?>&mdate=<?php echo $date ?>&species=<?php echo $_GET['species'] ?>&category=<?php echo $_GET['category'] ?>";
                            </script>
                            <?php
                        }
                } 
            }
        } else {
            ?>
            <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8;">
                <td class="text-center" colspan="6"><b> No Records Found!</b></td>
            </tr>
            <?php
        }
}


function modify_postmortem_lungs()
{
    $id     =   $_GET['mid'];
    $date     =   $_GET['mdate'];
    global $con;
    $sql = "SELECT pm_table.lung_weight as lung_weight, pm_table.lung_cause as lung_cause, disease_tbl.disease_description as disease_description,pm_table.lung_number_of_affected as lung_number_of_affected, pm_table.pm_id as pm_id FROM ddr_table LEFT JOIN pm_table on ddr_table.drr_id=pm_table.drr_id  INNER JOIN disease_tbl ON pm_table.lung_cause = disease_tbl.d_id WHERE pm_table.drr_id='$id' AND pm_table.pm_date='$date'";
    $result = $con->query($sql);
    if ($result->num_rows > 0) {
        while($row = $result->fetch_assoc()) {
            if($row['lung_weight'] == '0' || $row['lung_cause'] == '0' ){
            ?>
            <?php } else{ ?>
                <tr style="padding:10px !important; margin:px !important; border:1px solid #bdb8b8;">
                    <td style="font-size:12px !important; padding:2px;"><?php echo $row['disease_description']  ?></td>
                    <td style="font-size:12px !important; padding:2px;"><?php echo $row['lung_number_of_affected']  ?></td>
                    <td style="font-size:12px !important; padding:2px;"><?php echo $row['lung_weight']  ?></td>
                    <td style="font-size:12px !important; padding:2px;">
                    <button type="button" class="btn bg-danger btn-sm" data-toggle="modal" data-target="#delete_suspect<?php echo $row['pm_id'];?>"><i class="icon-cancel-square"></i></button>
                    </td>
                </tr>
                                <div id="delete_suspect<?php echo $row['pm_id'];  ?>" class="modal fade" tabindex="-1" data-backdrop="static">
                                    <div class="modal-dialog">
                                        <div class="modal-content">
                                            <div class="modal-header bg-primary">
                                                <h6 class="modal-title">Remove Lung Abnormalities</h6>
                                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                                            </div>
                                            <div class="modal-body">
                                            <form method="POST" class="form-horizontal">
                                                    <input type="hidden"  name="id" class="form-control" value="<?php echo $row['pm_id'];  ?>">
                                                    <h5 class="text-center">Are you sure you want to delete this  data?</h5>
                                            </div>
                                            <div class="modal-footer">
                                                <button type="button" class="btn btn-link" data-dismiss="modal">No</button>
                                                <button type="submit" class="btn bg-danger" name="btn_delete">Yes</button>
                                            </div>
                                        </div>
                                    </form>
                                    </div>
                                </div>
           <?php
                        if(isset($_POST['btn_delete']))
                        {
                            $sid   =   $_POST['id'];
                            $sql = mysqli_query($con, "DELETE FROM pm_table  WHERE pm_id='$sid'  ");
                            $delete_report_sql = mysqli_query($con, "DELETE FROM disease_report_tbl  WHERE drr_id='$sid'  ");
                            ?>
                            <script type="text/javascript">
                                window.location = "postmortem.php?med=<?php echo $_GET['med'] ?>&mid=<?php echo $id  ?>&mdate=<?php echo $date ?>&species=<?php echo $_GET['species'] ?>&category=<?php echo $_GET['category'] ?>";
                            </script>
                            <?php
                        }
                } 
            }
        } else {
            ?>
            <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8;">
                <td class="text-center" colspan="6"><b> No Records Found!</b></td>
            </tr>
            <?php
        }
}



function modify_postmortem_liver()
{
    $id     =   $_GET['mid'];
    $date     =   $_GET['mdate'];
    global $con;
    $sql = "SELECT pm_table.liver_weight as liver_weight, pm_table.liver_cause as liver_cause, pm_table.liver_number_of_affected as liver_number_of_affected,disease_tbl.disease_description as disease_description, pm_table.pm_id as pm_id FROM ddr_table LEFT JOIN pm_table on ddr_table.drr_id=pm_table.drr_id  INNER JOIN disease_tbl ON pm_table.liver_cause = disease_tbl.d_id  WHERE pm_table.drr_id='$id' AND pm_table.pm_date='$date'";
    $result = $con->query($sql);
    if ($result->num_rows > 0) {
        while($row = $result->fetch_assoc()) {
            if($row['liver_weight'] == '0' || $row['liver_cause'] == '0' ){
            ?>
            <?php } else{ ?>
                <tr style="padding:10px !important; margin:px !important; border:1px solid #bdb8b8;">
                    <td style="font-size:12px !important; padding:2px;"><?php echo $row['disease_description']  ?></td>
                    <td style="font-size:12px !important; padding:2px;"><?php echo $row['liver_weight']  ?></td>
                    <td style="font-size:12px !important; padding:2px;"><?php echo $row['liver_number_of_affected']  ?></td>
                    <td style="font-size:12px !important; padding:2px;">
                    <button type="button" class="btn bg-danger btn-sm" data-toggle="modal" data-target="#delete_suspect<?php echo $row['pm_id'];?>"><i class="icon-cancel-square"></i></button>
                    </td>
                </tr>
                                <div id="delete_suspect<?php echo $row['pm_id'];  ?>" class="modal fade" tabindex="-1" data-backdrop="static">
                                    <div class="modal-dialog">
                                        <div class="modal-content">
                                            <div class="modal-header bg-primary">
                                                <h6 class="modal-title">Remove Liver Abnormalities</h6>
                                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                                            </div>
                                            <div class="modal-body">
                                            <form method="POST" class="form-horizontal">
                                                    <input type="hidden"  name="id" class="form-control" value="<?php echo $row['pm_id'];  ?>">
                                                    <h5 class="text-center">Are you sure you want to delete this  data?</h5>
                                            </div>
                                            <div class="modal-footer">
                                                <button type="button" class="btn btn-link" data-dismiss="modal">No</button>
                                                <button type="submit" class="btn bg-danger" name="btn_delete">Yes</button>
                                            </div>
                                        </div>
                                    </form>
                                    </div>
                                </div>
           <?php
                    if(isset($_POST['btn_delete']))
                    {
                        $sid   =   $_POST['id'];
                        $sql = mysqli_query($con, "DELETE FROM pm_table  WHERE pm_id='$sid'  ");
                        $delete_report_sql = mysqli_query($con, "DELETE FROM disease_report_tbl  WHERE drr_id='$sid'  ");
                        ?>
                            <script type="text/javascript">
                                window.location = "postmortem.php?med=<?php echo $_GET['med'] ?>&mid=<?php echo $id  ?>&mdate=<?php echo $date ?>&species=<?php echo $_GET['species'] ?>&category=<?php echo $_GET['category'] ?>";
                        </script>
                        <?php
                    }
                } 
            }
        } else {
            ?>
            <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8;">
                <td class="text-center" colspan="6"><b> No Records Found!</b></td>
            </tr>
            <?php
        }
}



function modify_postmortem_heart()
{
    
    
    $id     =   $_GET['mid'];
    $date     =   $_GET['mdate'];

    global $con;

    $sql = "SELECT pm_table.heart_weight as heart_weight, pm_table.heart_cause as heart_cause,pm_table.heart_number_of_affected as heart_number_of_affected, disease_tbl.disease_description as disease_description,  pm_table.pm_id as pm_id FROM ddr_table LEFT JOIN pm_table on ddr_table.drr_id=pm_table.drr_id   INNER JOIN disease_tbl ON pm_table.heart_cause = disease_tbl.d_id  WHERE pm_table.drr_id='$id' AND pm_table.pm_date='$date'";
    $result = $con->query($sql);

    if ($result->num_rows > 0) {
        while($row = $result->fetch_assoc()) {


            if($row['heart_weight'] == '0' || $row['heart_cause'] == '0' ){
            ?>
           
