function show_me_drr()
{
    global $con;

    $sql = "SELECT * FROM meat_establishment_tbl WHERE me_status='Active'";
    $result = $con->query($sql);

    if ($result->num_rows > 0) {
    while($row = $result->fetch_assoc()) {
        ?>
        <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8;">
            <td><b> <?php echo $row['me_plant_name']  ?></b></td>
            <td><?php echo $row['me_expiry']  ?></td>
            <td><span class="badge badge-danger"><?php echo $row['me_status']  ?></span></td>
            <td><?php echo $row['me_remarks']  ?></td>
            <td><?php echo $row['me_date_created']  ?></td>
            <td>
                <a href="drrr.php?id=<?php echo $row['me_id'] ?>" class="btn btn-info btn-icon"><i class="icon-eye4"></i></a>
            </td>
        </tr>
        <?php

    }
    } else {
        ?>
        <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8;">
            <td class="text-center" colspan="6"><b> No Records Found!</b></td>
        </tr>
        <?php
    }
}



function login()
{
    global $con;
    if(isset($_POST['btn_login']))
    {
        $user = mysqli_real_escape_string($con, $_POST['user']);
        $pass = mysqli_real_escape_string($con, $_POST['pass']);
        $sql = mysqli_query($con, "SELECT * FROM employee_tbl WHERE username='$user'");
        $row = mysqli_fetch_assoc($sql);


        if($row)
        {
            if($row['password'] != $pass)
            {
                ?> 
                <div class="alert alert-danger border-0 alert-dismissible">
                    <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                    <span class="font-weight-semibold">Password Incorrect</a>.
                </div>
                <?php
            }
            else
            {

                if($row['account_activation'] =='0')
                  {
                    ?>
                            <div class="alert alert-danger border-0 alert-dismissible">
                                <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                <span class="font-weight-semibold">Please check your inbox to activate email</a>.
                            </div>
                    <?php
                  }
                  else
                  {
            
                      if($row['account_status'] == 'Rejected')
                        {
                            ?> 
                                <div class="alert alert-danger border-0 alert-dismissible">
                                    <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                    <span class="font-weight-semibold">Your application was rejected</a>.
                                </div>
                            <?php
                       }
                       else if($row['account_status'] == 'Pending')
                       {
                            ?> 
                                <div class="alert alert-danger border-0 alert-dismissible">
                                    <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                    <span class="font-weight-semibold">Please wait for the approval of your application</a>.
                                </div>
                            <?php
                       }
                       else{
                            session_start();
                            $_SESSION['employee_id'] = $row['employee_id'];
                            $_SESSION['firstname'] = $row['firstname'];
                            $_SESSION['lastname'] = $row['lastname'];
                            $_SESSION['role'] = $row['role'];
                            $_SESSION['plant_name'] = $row['plant_name'];
                            $_SESSION['region'] = $row['region'];
                            $_SESSION['email'] = $row['email'];
                            $_SESSION['contact'] = $row['contact'];

                            if($row['role'] == 3)
                            {
                                header("location: ../../pages/me/dashboard.php");
                            }
                            else if($row['role'] == 2)
                            {
                                 


                                header("location: ../../pages/mio/dashboard.php");

                            }
                            else if($row['role'] == 1)
                            {
                                header("location: ../../pages/posms/dashboard.php");
                            }
                            else if($row['role'] == 0)
                            {
                                header("location: ../../pages/posmd/dashboard.php");
                            }
                        }
              }
            }
        }
        else{
            ?> 
            <div class="alert alert-danger border-0 alert-dismissible">
                <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                <span class="font-weight-semibold">Username doesn't Exist</a>.
            </div>
            <?php
        }
    }
}



function login_me()
{
    global $con;
    if(isset($_POST['btn_logins']))
    {
        $user = mysqli_real_escape_string($con, $_POST['user']);
        $pass = mysqli_real_escape_string($con, $_POST['pass']);
        $sql = mysqli_query($con, "SELECT * FROM meat_establishment_tbl WHERE me_username='$user'");
        $row = mysqli_fetch_assoc($sql);


        if($row)
        {
            if($row['me_password'] != $pass)
            {
                ?> 
                    <div class="alert alert-danger border-0 alert-dismissible">
                        <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                        <span class="font-weight-semibold">Password Incorrect</a>.
                    </div>
                <?php
            }
            else
            {
                    session_start();
                    $_SESSION['me_id'] = $row['me_id'];
                    $_SESSION['me_plant_name'] = $row['me_plant_name'];
                    $_SESSION['me_type'] = $row['me_type'];
                    $_SESSION['me_region_code'] = $row['me_region_code'];
                    $_SESSION['me_lto_number'] = $row['me_lto_number'];
                    $_SESSION['me_email'] = $row['me_email'];
                    $_SESSION['me_status'] = $row['me_status'];
                    $_SESSION['me_remarks'] = $row['me_remarks'];
                    $_SESSION['me_account_activation'] = $row['me_account_activation'];
                    if($row['me_account_activation'] == 0 || $row['me_status'] == 'Pending' || $row['me_status'] =='Rejected')
                    {
                        header("location: ../../pages/me/meat_establishment_activation.php");
                    }   
                    else
                    {
                        header("location: ../../pages/me/dashboard.php");
                    }
            }
        }
        else{
            ?> 
            <div class="alert alert-danger border-0 alert-dismissible">
                <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                <span class="font-weight-semibold">Username doesn't Exist</a>.
            </div>
            <?php
        }
    }
}



function logout(){
    if(isset($_POST['logout'])){
                session_start();
                $id = $_POST['logout_name'];
        session_destroy();
        session_unset($id);
        header('location:pages/auth/index.php');

    }


  
}

?>