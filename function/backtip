                        <?php } ?>



                      



                      <?php 
                        $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Horse'   AND fit_province='$province'";
                        $cattle_result = $con->query($cattle_sql);
                        while($cattle_row = $cattle_result->fetch_assoc()){
                        ?>
                        <td  style="background-color: white !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                        <td style="background-color: #bdbaba !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>
                        <?php } ?>


                        <?php 
                        $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Goat'  AND fit_province='$province'";
                        $cattle_result = $con->query($cattle_sql);
                        while($cattle_row = $cattle_result->fetch_assoc()){
                        ?>
                        <td style="background-color: white !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                        <td style="background-color: #bdbaba !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>
                        <?php } ?>


                        <?php 
                        $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Sheep '  AND fit_province='$province'";
                        $cattle_result = $con->query($cattle_sql);
                        while($cattle_row = $cattle_result->fetch_assoc()){
                        ?>
                        <td style="background-color: white !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                        <td style="background-color: #bdbaba !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>
                        <?php } ?>


                </tr>

                <?php
                }
                }
        else{
            ?>
                <td colspan="17" class="text-center" style="color: white;">NO DATA</td>
            <?php 
        }

}



function posms_accounts()
{
    global $con;
    $region = $_SESSION['region'];
    $sql = "SELECT * FROM employee_tbl WHERE role='1'";
    $result = $con->query($sql);
    while($row = $result->fetch_assoc())
    {
        ?>
            <tr >
                <td  style="width:120px;"><?php echo $row['employee_code']; ?></td>
                <td  ><?php echo $row['lastname'].', '.$row['firstname'] ?></td>
                <td  ><?php echo $row['email']; ?></td>
                <td  ><?php echo $row['contact']; ?></td>
                <td  ><?php echo $row['username']; ?></td>
                <td style="width: 250px;"><?php echo $row['region']; ?></td>
                <td  >
                    <?php 
                    if($row['account_status']== '1'){
                        ?><span class="badge bg-success">Active</span><?php
                    }
                    else{
                        ?><span class="badge bg-danger">Deactivated</span><?php
                    }
                    ?>
                </td>
                <td style="">
                

                <?php
                if(isset($_POST['btn_update_posmd_account']))
                {
                    $emp_id = $_POST['emp_id'];
                    $emp_code = $_POST['emp_code'];
                    $firstname = $_POST['firstname'];
                    $lastname = $_POST['lastname'];
                    $username = $_POST['username'];
                    $email = $_POST['email'];
                    $contact = $_POST['contact'];
                    $status = $_POST['status'];
                    $sql = mysqli_query($con, "UPDATE employee_tbl SET employee_code='$emp_code', firstname='$firstname', lastname='$lastname', email='$email', contact='$contact', username='$username', account_status='$status' WHERE employee_id='$emp_id'");
                    ?>
                        <script type="text/javascript">
                            window.location = "posms_account.php";
                        </script>
                    <?php
                }
                ?>
                </td>
            </tr>
        <?php
    }
}

function add_posmd_account()
{
    global $con;
    if(isset($_POST['btn_add_posmd']))
    {
        $emp_code = mysqli_real_escape_string($con, $_POST['emp_code']);
        $firstname = mysqli_real_escape_string($con, $_POST['firstname']);
        $lastname = mysqli_real_escape_string($con, $_POST['lastname']);
        $username = mysqli_real_escape_string($con, $_POST['username']);
        $password = mysqli_real_escape_string($con, $_POST['password']);
            
        $sql = mysqli_query($con, "INSERT INTO employee_tbl (employee_code,firstname,lastname,username,password,role)
        VALUES('$emp_code','$firstname','$lastname','$username','$password','0')");
        ?>
        <script type="text/javascript">
            window.location = "posmd_account.php";
        </script>
        <?php
    }
}


function add_posms_account()
{
    global $con;
    if(isset($_POST['btn_add_posms']))
    {
        $emp_code = mysqli_real_escape_string($con, $_POST['emp_code']);
        $firstname = mysqli_real_escape_string($con, $_POST['firstname']);
        $lastname = mysqli_real_escape_string($con, $_POST['lastname']);
        $me_region = mysqli_real_escape_string($con, $_POST['me_region']);


        $username = mysqli_real_escape_string($con, $_POST['username']);
        $password = mysqli_real_escape_string($con, $_POST['password']);
        $contact = mysqli_real_escape_string($con, $_POST['contact']);
        $email = mysqli_real_escape_string($con, $_POST['email']);
        $region = $_SESSION['region'];
            
        $sql = mysqli_query($con, "INSERT INTO employee_tbl (employee_code,firstname,lastname,email,username,password,region,role,contact,account_activation)
        VALUES('$emp_code','$firstname','$lastname','$email','$username','$password','$me_region','1','$contact','1')");
        ?>
        <script type="text/javascript">
            window.location = "posms_account.php";
        </script>
        <?php
    }
}

