function show_antemortem_details()
{

       
    $id     =   $_GET['mid'];
    $date     =   $_GET['mdate'];

    global $con;

    $sql = "SELECT * FROM ddr_table INNER JOIN meat_establishment_tbl on ddr_table.me_id = meat_establishment_tbl.me_id WHERE drr_id='$id' AND drr_date='$date'";
    $result = $con->query($sql);

    $row = $result->fetch_assoc();

    ?>
            <li class="nav-item">
                <a class="nav-link" style="color:black;">
                    Establishment name
                    <span class=" font-size-sm font-weight-normal ml-auto"> <?php echo $row['me_plant_name']; ?></span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" style="color:black;">
                     Meat Dealer
                    <span class=" font-size-sm font-weight-normal ml-auto"><?php echo $row['drr_meat_dealer']; ?></span>
                </a>
            </li>
           <li class="nav-item">
                <a class="nav-link" style="color:black;">
                    Date
                    <span class=" font-size-sm font-weight-normal ml-auto"><?php echo $row['drr_date']; ?></span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" style="color:black;">
                     Species
                    <span class=" font-size-sm font-weight-normal ml-auto"><?php echo $row['drr_species']; ?></span>
                </a>
            </li>
    <?php
}



function modify_antemortem_reject()
{

    
    $id     =   $_GET['mid'];
    $date     =   $_GET['mdate'];

    global $con;

    $sql = "SELECT am_table.r_heads as r_heads, am_table.r_weight as r_weight, am_table.r_cause as r_cause, disease_tbl.disease_description as disease_description, am_table.am_table as am_table FROM ddr_table INNER JOIN am_table on ddr_table.drr_id=am_table.drr_id  INNER JOIN disease_tbl ON am_table.r_cause=disease_tbl.d_id WHERE ddr_table.drr_id='$id' AND am_table.am_date='$date'";
    $result = $con->query($sql);

    if ($result->num_rows > 0) {
        while($row = $result->fetch_assoc()) {
            if($row['r_heads'] == '0' || $row['r_weight'] == '0' || $row['r_cause'] == '0' ){  }else{ ?>
            <tr style="padding:10px !important; margin:px !important; border:1px solid #bdb8b8;">
                <td style="font-size:12px !important; "><b> <?php echo number_format($row['r_heads'])  ?></b></td>
                <td style="font-size:12px !important; padding:2px;"><?php echo number_format($row['r_weight'],2)  ?></td>
                <td style="font-size:12px !important; padding:2px;"><?php echo $row['disease_description']  ?></td>
                <td style="font-size:12px !important; padding:2px;">
                    <button type="button" class="btn bg-danger btn-sm" data-toggle="modal" data-target="#delete_rejected<?php echo $row['am_table'];?>"><i class="icon-cancel-square"></i></button>
                </td>
            </tr>

                    <div id="delete_rejected<?php echo $row['am_table'];  ?>" class="modal fade" tabindex="-1" data-backdrop="static">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header bg-primary">
                                    <h6 class="modal-title">Rejected</h6>
                                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                                </div>

                                <div class="modal-body">
                                <form method="POST" class="form-horizontal">
                                           
                                        <input type="hidden"  name="id" class="form-control" value="<?php echo $row['am_table'];  ?>">

                                        <h5 class="text-center">Are you sure you want to delete this condemned data?</h5>

                                </div>

                                <div class="modal-footer">
                                    <button type="button" class="btn btn-link" data-dismiss="modal">No</button>
                                    <button type="submit" class="btn bg-danger" name="btn_delete">Yes</button>
                                </div>
                            </div>
                        </form>
                        </div>
                    </div>

                </td>
            <?php
                        if(isset($_POST['btn_delete']))
                        {
                            $sid   =   $_POST['id'];

                            $sql = mysqli_query($con, "DELETE FROM am_table  WHERE am_table='$sid'  ");

                            $delete_report_sql = mysqli_query($con, "DELETE FROM disease_report_tbl  WHERE drr_id='$sid'  ");
                            ?>
                                <script type="text/javascript">
                                    window.location = "antemortem.php?med=<?php echo $_GET['med']  ?>&mid=<?php echo $id  ?>&mdate=<?php echo $date ?>&species=<?php echo $_GET['species'] ?>&category=<?php echo $_GET['category'] ?>";
                            </script>
                            <?php
                        }
                }
            }
        } else {
            ?>
            <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8;">
                <td class="text-center" colspan="6"><b> No Records Found!</b></td>
            </tr>
            <?php


        }

    
   
}
function modify_antemortem_condemned()
{

    
    $id     =   $_GET['mid'];
    $date     =   $_GET['mdate'];

    global $con;

    $sql = "SELECT am_table.c_heads as c_heads, am_table.am_table as am_table, am_table.c_weight as c_weight,disease_tbl.disease_description as disease_description, am_table.c_cause as c_cause FROM ddr_table LEFT JOIN am_table on ddr_table.drr_id=am_table.drr_id INNER JOIN disease_tbl ON am_table.c_cause=disease_tbl.d_id WHERE ddr_table.drr_id='$id' AND am_table.am_date='$date'";
    $result = $con->query($sql);

    if ($result->num_rows > 0) {
        while($row = $result->fetch_assoc()) {

            if($row['c_heads'] == '0' || $row['c_weight'] == '0' || $row['c_cause'] == '0' ){  }else{ ?>
            <tr style="padding:10px !important; margin:px !important; border:1px solid #bdb8b8;">
                <td style="font-size:12px !important; "><b> <?php echo number_format($row['c_heads'])  ?></b></td>
                <td style="font-size:12px !important; padding:2px;"><?php echo number_format($row['c_weight'],2)  ?></td>
                <td style="font-size:12px !important; padding:2px;"><?php echo $row['disease_description']  ?></td>
                <td style="font-size:12px !important; padding:2px; width:150px;">
                    <button type="button" class="btn bg-danger btn-sm" data-toggle="modal" data-target="#delete_condemned<?php echo $row['am_table'];?>"><i class="icon-cancel-square"></i></button>



                    <div id="delete_condemned<?php echo $row['am_table'];  ?>" class="modal fade" tabindex="-1" data-backdrop="static">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header bg-primary">
                                    <h6 class="modal-title">Condemned</h6>
                                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                                </div>

                                <div class="modal-body">
                                <form method="POST" class="form-horizontal">
                                           
                                        <input type="hidden"  name="id" class="form-control" value="<?php echo $row['am_table'];  ?>">

                                        <h5 class="text-center">Are you sure you want to delete this condemned data?</h5>

                                </div>

                                <div class="modal-footer">
                                    <button type="button" class="btn btn-link" data-dismiss="modal">No</button>
                                    <button type="submit" class="btn bg-danger" name="btn_delete">Yes</button>
                                </div>
                            </div>
                        </form>
                        </div>
                    </div>

                </td>
            </tr>
            <?php

                if(isset($_POST['btn_delete']))
                {
                    $sid   =   $_POST['id'];

                    $sql = mysqli_query($con, "DELETE FROM am_table  WHERE am_table='$sid'  ");
                            $delete_report_sql = mysqli_query($con, "DELETE FROM disease_report_tbl  WHERE drr_id='$sid'  ");

                    ?>
                        <script type="text/javascript">
                            window.location = "antemortem.php?med=<?php echo $_GET['med']  ?>&mid=<?php echo $id  ?>&mdate=<?php echo $date ?>&species=<?php echo $_GET['species'] ?>&category=<?php echo $_GET['category'] ?>";
                    </script>
                    <?php
                }
            }
        }
        } else {
            ?>
            <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8;">
                <td class="text-center" colspan="6"><b> No Records Found!</b></td>
            </tr>
            <?php
        }
}

function add_antemortem_suspected()
{
    global $con;
    date_default_timezone_set('Asia/Manila');
    $today = date("m/d/Y h:i:sa");
    $date_ap = date("m/d/Y");
    $time = date("h:i:sa");
    if(isset($_POST['btn_submit']))
    {   
        $id     =   mysqli_real_escape_string($con, $_POST['id']);
        $plant_name     =   $_GET['med'];
        $heads     =   mysqli_real_escape_string($con, $_POST['heads']);
        $weight     =   mysqli_real_escape_string($con, $_POST['weight']);
        $cause     =   mysqli_real_escape_string($con, $_POST['cause']);
        $date     =   mysqli_real_escape_string($con, $_POST['date']);
        $med     =   $_GET['med'];

        $sql = mysqli_query($con, "INSERT INTO am_table (drr_id,s_heads,s_weight,s_cause,am_date,me_id)
        VALUES('$id','$heads','$weight','$cause','$date_ap','$plant_name')");
        $last_id = $con->insert_id;

        $sql = mysqli_query($con, "INSERT INTO disease_report_tbl (d_id,dr_date,drr_id)
        VALUES('$cause','$date_ap','$last_id')");
    ?>
       <script type="text/javascript">
           window.location = "antemortem.php?med=<?php echo $med  ?>&mid=<?php echo $id  ?>&mdate=<?php echo $date ?>&species=<?php echo $_GET['species'] ?>&category=<?php echo $_GET['category'] ?>";
      </script>
    <?php
    }
}

function add_postmortem_carcass()
{
    date_default_timezone_set('Asia/Manila');
    $today = date("m/d/Y h:i:sa");
    $date_ap = date("m/d/Y");
    $time = date("h:i:sa");
    global $con;

    if(isset($_POST['btn_submits']))
    {   
        $id     =   $_POST['id'];
        $date     =   $_POST['date'];
        $heads     =   $_POST['heads'];
        $weight     =   $_POST['weight'];
        $cause     =   $_POST['cause'];
        $med = $_GET['med'];

        
        $sql = mysqli_query($con, "INSERT INTO pm_table (drr_id,carcass_number_of_heads,carcass_weight,carcass_cause,pm_date,me_id)
        VALUES('$id','$heads','$weight','$cause','$date_ap','$med')");
        $last_id = $con->insert_id;
        $sql = mysqli_query($con, "INSERT INTO disease_report_tbl (d_id,dr_date,drr_id)
        VALUES('$cause','$date_ap','$last_id')");
    ?>
       <script type="text/javascript">
           window.location = "postmortem.php?med=<?php echo $_GET['med'] ?>&mid=<?php echo $id  ?>&mdate=<?php echo $date?>&species=<?php echo $_GET['species'] ?>&category=<?php echo $_GET['category'] ?>";
      </script>
    <?php
        
        

    }
}

