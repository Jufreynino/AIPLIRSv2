function mmir_posmd_me(){
    global $con;
    $region = $_SESSION['region'];

    $from = $_GET['from'];
    $fromdate = DateTime::createFromFormat('Y-m-d',$from);
    $from_date = $fromdate->format('m/d/Y');


    $to = $_GET['to'];
    $todate = DateTime::createFromFormat('Y-m-d',$to);
    $to_date = $todate->format('m/d/Y');

    $me_id = $_GET['me_id'];

    $sql = "SELECT * FROM fit_human_consumption INNER JOIN meat_establishment_tbl ON fit_human_consumption.me_id=meat_establishment_tbl.me_id WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND meat_establishment_tbl.me_id='$me_id' GROUP BY me_plant_name ";
    $result = $con->query($sql);
    if(mysqli_num_rows($result) > 0)
    {
        while($row = $result->fetch_assoc())
                {
                    ?>
                    <tr>
                           <td   class="sticky-col first-col" style=" background-color:white !important; color:black;"><?php echo $row['me_plant_name']; ?></td>
                            <td  style="color:black; background-color:white !important; "><?php echo $row['me_type']?></td>
                            <td  style="color:black; background-color:white !important; "><?php echo $row['me_lto_number']; ?></td>



                            <?php 
                            $hog_sql = "SELECT SUM(fit_number_of_head) as hog_fit_number_of_head, SUM(fit_weight) as hog_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Hog'  AND me_id='$me_id'";
                            $hog_result = $con->query($hog_sql);
                            while($hog_row = $hog_result->fetch_assoc()){
                            ?>
                            <td  style="background-color:white;" class="text-right"><?php echo number_format($hog_row['hog_fit_number_of_head']); ?></td>
                            <td style="background-color: #bdbaba !important;" class="text-right"><?php echo number_format($hog_row['hog_fit_weight']); ?></td>
                            <?php } ?>





                                            <?php 
                            $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Chicken' AND me_id='$me_id'";
                            $cattle_result = $con->query($cattle_sql);
                            while($cattle_row = $cattle_result->fetch_assoc()){
                            ?>
                            <td  style="background-color:white;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                            <td style="background-color: #bdbaba !important;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>
                            <?php } ?>






                            <?php 

                            $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Cattle'  AND me_id='$me_id'";
                            $cattle_result = $con->query($cattle_sql);
                            while($cattle_row = $cattle_result->fetch_assoc()){

                            ?>
                            <td  style="background-color:white !important;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                            <td style="background-color:#bdbaba !important;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>



                            <?php } ?>

                             <?php 

                            $carabao_sql = "SELECT SUM(fit_number_of_head) as carabao_fit_number_of_head, SUM(fit_weight) as carabao_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Carabao'  AND me_id='$me_id'";
                            $carabao_result = $con->query($carabao_sql);
                            while($carabao_row = $carabao_result->fetch_assoc()){

                            ?>
                            <td  style="background-color:white;" class="text-right"><?php echo number_format($carabao_row['carabao_fit_number_of_head']); ?></td>
                            <td style="background-color: #bdbaba !important;" class="text-right"><?php echo number_format($carabao_row['carabao_fit_weight']); ?></td>

                            <?php } ?>





                          <?php 
                            $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Horse'  AND me_id='$me_id'";
                            $cattle_result = $con->query($cattle_sql);
                            while($cattle_row = $cattle_result->fetch_assoc()){
                            ?>
                            <td  style="background-color:white;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                            <td style="background-color: #bdbaba !important;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>
                            <?php } ?>


                            <?php 
                            $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Goat'  AND me_id='$me_id'";
                            $cattle_result = $con->query($cattle_sql);
                            while($cattle_row = $cattle_result->fetch_assoc()){
                            ?>
                            <td  style="background-color:white;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                            <td style="background-color: #bdbaba !important;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>
                            <?php } ?>


                            <?php 
                            $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Sheep'  AND me_id='$me_id'";
                            $cattle_result = $con->query($cattle_sql);
                            while($cattle_row = $cattle_result->fetch_assoc()){
                            ?>
                            <td  style="background-color:white;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                            <td style="background-color: #bdbaba !important;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>
                            <?php } ?>


            

                    </tr>

                    <?php
                }
    }
    else
    {
        ?>
            <td colspan="17" class="text-center" style="color: white;">NO DATA</td>
        <?php
    }
