        while($row = mysqli_fetch_assoc($result)){

            $sqls    =    "SELECT SUM(pm_table.carcass_number_of_heads) as carcass_number_of_heads,SUM(pm_table.carcass_weight) as carcass_weights, disease_tbl.disease_description as disease_description FROM disease_tbl LEFT JOIN pm_table ON disease_tbl.d_id = pm_table.carcass_cause WHERE disease_level='Mid' AND pm_table.carcass_cause='".$row['carcass_cause']."' GROUP BY disease_tbl.disease_description";
                    $results = mysqli_query($con, $sqls); 
                    while($rows = mysqli_fetch_assoc($results)){

                    ?>
                    <tr>
                        <td ><?php echo $rows['disease_description'] ?></td>
                        <td class="text-right"><?php echo $rows['carcass_number_of_heads'] ?></td>
                        <td class="text-right"><?php echo $rows['carcass_weights'] ?></td>
                    </tr>
