function display_meat_dealer_info()
{
    global $con;
    $id = $_GET['emp_id'];
    $region = $_SESSION['region'];
    $sql = "SELECT * FROM employee_tbl WHERE employee_id='$id'";
    $result = $con->query($sql);
    $row = $result->fetch_assoc();
    ?>
        <h2 class="card-title text-center">
            <b><?php echo $row['lastname'].', '.$row['firstname'] ?></b><br></h2>
             <p class="text-center">Meat Inspector Officer</p>

             <table>
                 <tr>
                     <th>Employee Number</th>
                     <td><?php echo $row['employee_code'] ?></td>
                 </tr>
                  <tr>
                     <th>Email</th>
                     <td><?php echo $row['email'] ?></td>
                 </tr>
                  <tr>
                     <th>Contact #</th>
                     <td><?php echo $row['contact'] ?></td>
                 </tr>
                 <tr>
                     <th>Username</th>
                     <td><?php echo $row['username'] ?></td>
                 </tr>

             </table>
        
    <?php
}

function display_meat_dealer_assigned_mio()
{
      global $con;
        $id = $_GET['emp_id'];
        $region = $_SESSION['region'];
        $sql = "SELECT * FROM meat_establishment_tbl INNER JOIN employee_tbl ON meat_establishment_tbl.employee_id= employee_tbl.employee_id WHERE meat_establishment_tbl.employee_id='$id'";
        $result = $con->query($sql);
        while($row = $result->fetch_assoc())
        {
            ?>
                <tr >
                    <td style=""><?php echo $row['me_plant_name']; ?></td>
                    <td style=""><?php echo $row['me_type']; ?></td>
                    
                    <td style=""><?php echo $row['me_lto_number']?></td>
                    <td style=""><?php echo $row['me_expiry']; ?></td>
                </tr>

            <?php 
        }
}
function mio_accounts()
{
    global $con;
    $region = $_SESSION['region'];
    $sql = "SELECT * FROM employee_tbl WHERE role='2' AND region='$region'";
    $result = $con->query($sql);
    while($row = $result->fetch_assoc())
    {
        ?>
            <tr >
                <td style="width: 70px;"><?php echo $row['employee_code']; ?></td>
                <td style="width: 100px;"><?php echo $row['lastname'].', '.$row['firstname'] ?></td>
                <td style="width: 150px;"><?php echo $row['email']; ?></td>
                <td style="width: 150px;"><?php echo $row['username']; ?></td>
                <td style="width: 70px;">
                    <?php 
                    if($row['account_activation']== '0'){
                        ?><span class="badge bg-danger">Email Not Activated</span><?php
                    }
                    else{

                        if($row['account_status'] == 'Pending'){
                        ?><span class="badge bg-warning">Pending</span><?php
                        }
                        else if($row['account_status'] == 'Rejected')
                        {
                        ?><span class="badge bg-danger">Deactivated</span><?php

                        }
                        else if($row['account_status'] == 'Deactivated')
                        {
                        ?><span class="badge bg-danger">Deactivated</span><?php

                        }
                        else{
                        ?><span class="badge bg-success">Active</span><?php

                        }

                    }
                    ?>
                </td>
                <td style="width:150px;">


                <?php if($row['account_activation']== '0'){?>    
               

                <?php } else { ?>
                       <?php if($row['account_status'] == 'Pending'){ ?>
                            <button  type="button" class="btn bg-success" data-toggle="modal" data-target="#approve_mio_accouts<?php echo $row['employee_id'] ?>">Approve</button>



                            <button disabled type="button" class="btn bg-info" data-toggle="modal" data-target="#edit_posmd_accouts<?php echo $row['employee_id'] ?>">Edit</button>

                            <a style="pointer-events: none; cursor: default; opacity: .65;"  href="assigned_meat_establishment.php?emp_id=<?php echo $row['employee_id'] ?>" class="btn bg-teal">Assigned ME</a>

                        <?php }  else if($row['account_status'] == 'Deactivated') { ?>
                             <button  type="button" class="btn bg-success" data-toggle="modal" data-target="#approve_mio_accouts<?php echo $row['employee_id'] ?>">Approve</button>



                            <button disabled type="button" class="btn bg-info" data-toggle="modal" data-target="#edit_posmd_accouts<?php echo $row['employee_id'] ?>">Edit</button>

                            <a style="pointer-events: none; cursor: default; opacity: .65;"  href="assigned_meat_establishment.php?emp_id=<?php echo $row['employee_id'] ?>" class="btn bg-teal">Assigned ME</a>

                        <?php } else{ ?>

                             <button disabled type="button" class="btn bg-success" data-toggle="modal" data-target="#approve_mio_accouts<?php echo $row['employee_id'] ?>">Approve</button>



                            <button  type="button" class="btn bg-info" data-toggle="modal" data-target="#edit_posmd_accouts<?php echo $row['employee_id'] ?>">Edit</button>

                            <a  href="assigned_meat_establishment.php?emp_id=<?php echo $row['employee_id'] ?>" class="btn bg-teal">Assigned ME</a>

                        <?php } ?>
                <?php } ?>

                
                <div id="approve_mio_accouts<?php echo $row['employee_id'] ?>" class="modal fade" tabindex="-1">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header bg-success">
                                <h6 class="modal-title">Approve MIO Account</h6>
                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                            </div>
                            <form method="POST">
                            <div class="modal-body">
                                <input type="hidden" name="e_id" value="<?php echo $row['employee_id'] ?>">
                                    <h5 class="text-center">Are you sure you want to approved this appilcation?</h5>                                
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-link" data-dismiss="modal">Close</button>
                                <button type="submit" name="btn_approved_mio_account" class="btn bg-success">Approve</button>
                            </div>
                        </div>
                    </form>
                    </div>
                </div>



                <div id="edit_posmd_accouts<?php echo $row['employee_id'] ?>" class="modal fade" tabindex="-1">
					<div class="modal-dialog">
						<div class="modal-content">
							<div class="modal-header bg-info">
								<h6 class="modal-title">Edit POSMS Account</h6>
								<button type="button" class="close" data-dismiss="modal">&times;</button>
							</div>
                            <form method="POST">
							<div class="modal-body">
                                        <div class="form-group">
                                            
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    <label>Employee Code</label>
                                                    <input type="hidden" value="<?php echo $row['employee_id'] ?>" name="emp_id" class="form-control" required>
                                                    <input type="text" value="<?php echo $row['employee_code'] ?>" name="emp_code" class="form-control" required>
                                                </div>
                                            </div>
                                        </div>
                                        
                                        <div class="form-group">
                                            <div class="row">
                                                <div class="col-sm-6">
                                                    <label>First name</label>
                                                    <input type="text" value="<?php echo $row['firstname'] ?>"  name="firstname" class="form-control" required>
                                                </div>

                                                <div class="col-sm-6">
                                                    <label>Last name</label>
                                                    <input type="text" value="<?php echo $row['lastname'] ?>"  name="lastname" class="form-control" required>
                                                </div>
                                            </div>
                                        </div>


                                        <div class="form-group">
                                            <div class="row">
                                                <div class="col-sm-6">
                                                    <label>Username</label>
                                                    <input type="text" value="<?php echo $row['username'] ?>"  name="username" class="form-control" required>
                                                </div>

                                                <div class="col-sm-6">
                                                    <label>Email</label>
                                                    <input type="text" value="<?php echo $row['email'] ?>"  name="email" class="form-control" required>
                                                </div>
                                            </div>
                                        </div>
                                        <br>
                                        <div class="form-group">
                                            <label class="d-block font-weight-semibold">Status of Account</label>
                                            <?php 
                                            if($row['account_status']== 'Active')
                                                {
                                                    ?>
                                                        <div class="form-check form-check-inline">
                                                            <label class="form-check-label">
                                                                <input type="radio" class="form-check-input" name="status" value="Active" checked>
                                                                Active
                                                            </label>
                                                        </div>

                                                        <div class="form-check form-check-inline">
                                                            <label class="form-check-label">
                                                                <input type="radio" class="form-check-input" name="status" value="Deactivated">
                                                            Deactivate
                                                            </label>
                                                        </div>

                                                    <?php
                                                }
                                            else
                                                {
                                                    ?>
                                                        <div class="form-check form-check-inline">
                                                            <label class="form-check-label">
                                                                <input type="radio" class="form-check-input" name="status" value="Active" >
                                                                Active
                                                            </label>
                                                        </div>

                                                        <div class="form-check form-check-inline">
                                                            <label class="form-check-label">
                                                                <input type="radio" class="form-check-input" name="status" value="Deactivated"checked>
                                                            Deactivate
                                                            </label>
                                                        </div>
                                                    <?php
                                                }
                                            ?>
                                        </div>
							</div>
							<div class="modal-footer">
								<button type="button" class="btn btn-link" data-dismiss="modal">Close</button>
								<button type="submit" name="btn_update_posmd_account" class="btn bg-info">Save changes</button>
							</div>
						</div>
                    </form>
					</div>
				</div>
                <?php
                if(isset($_POST['btn_update_posmd_account']))
                {
                    $emp_id = $_POST['emp_id'];
                    $emp_code = $_POST['emp_code'];
                    $firstname = $_POST['firstname'];
                    $lastname = $_POST['lastname'];
                    $username = $_POST['username'];
                    $email = $_POST['email'];
                    $status = $_POST['status'];
                    $sql = mysqli_query($con, "UPDATE employee_tbl SET employee_code='$emp_code', firstname='$firstname', lastname='$lastname', email='$email', username='$username', account_status='$status' WHERE employee_id='$emp_id' ");
                    ?>
                        <script type="text/javascript">
                            window.location = "meat_inspector_account.php";
                        </script>
                    <?php
                }


                if(isset($_POST['btn_approved_mio_account']))
                {
                    $e_id = $_POST['e_id'];
                    $sql = mysqli_query($con, "UPDATE employee_tbl SET account_status='Active' WHERE employee_id='$e_id' ");
                    ?>
                        <script type="text/javascript">
                            window.location = "meat_inspector_account.php";
                        </script>
                    <?php
                }

                
                ?>
                </td>
            </tr>
        <?php
    }

}

function mmir_posms_city_me (){
    global $con;
    $region = $_SESSION['region'];

    $from = $_GET['from'];
    $fromdate = DateTime::createFromFormat('Y-m-d',$from);
    $from_date = $fromdate->format('m/d/Y');


    $to = $_GET['to'];
    $todate = DateTime::createFromFormat('Y-m-d',$to);
    $to_date = $todate->format('m/d/Y');

    $province = $_GET['province'];
    $city = $_GET['city'];

    $sql = "SELECT * FROM fit_human_consumption INNER JOIN meat_establishment_tbl ON fit_human_consumption.me_id=meat_establishment_tbl.me_id WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND fit_province='$province' AND fit_region='$region' AND fit_city='$city' GROUP BY me_plant_name ";
    $result = $con->query($sql);
    if(mysqli_num_rows($result) > 0)
    {
        while($row = $result->fetch_assoc())
                {
                    ?>
                    <tr>
                           <td   class="sticky-col first-col" style=" background-color:#253137 !important; color:White;"><?php echo $row['me_plant_name']; ?></td>
                            <td  style="color:white"><?php echo $row['me_type']?></td>
                            <td  style="color:white"><?php echo $row['me_lto_number']; ?></td>



                            <?php 

                            $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Cattle'  AND fit_region='$region' AND fit_province='$province' AND fit_city='$city'";
                            $cattle_result = $con->query($cattle_sql);
                            while($cattle_row = $cattle_result->fetch_assoc()){

                            ?>
                            <td  style="background-color:white !important;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                            <td style="background-color:#bdbaba  !important;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>



                            <?php } ?>

                             <?php 

                            $carabao_sql = "SELECT SUM(fit_number_of_head) as carabao_fit_number_of_head, SUM(fit_weight) as carabao_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Carabao'  AND fit_region='$region' AND fit_province='$province' AND fit_city='$city'";
                            $carabao_result = $con->query($carabao_sql);
                            while($carabao_row = $carabao_result->fetch_assoc()){

                            ?>
                            <td  style="background-color:white;" class="text-right"><?php echo number_format($carabao_row['carabao_fit_number_of_head']); ?></td>
                            <td style="background-color: #bdbaba  !important;" class="text-right"><?php echo number_format($carabao_row['carabao_fit_weight']); ?></td>

                            <?php } ?>



                            <?php 
                            $hog_sql = "SELECT SUM(fit_number_of_head) as hog_fit_number_of_head, SUM(fit_weight) as hog_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Hog'  AND fit_region='$region' AND fit_province='$province' AND fit_city='$city'";
                            $hog_result = $con->query($hog_sql);
                            while($hog_row = $hog_result->fetch_assoc()){
                            ?>
                            <td  style="background-color:white;" class="text-right"><?php echo number_format($hog_row['hog_fit_number_of_head']); ?></td>
                            <td style="background-color: #bdbaba  !important;" class="text-right"><?php echo number_format($hog_row['hog_fit_weight']); ?></td>
                            <?php } ?>



                          <?php 
                            $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Horse'  AND fit_region='$region' AND fit_province='$province' AND fit_city='$city'";
                            $cattle_result = $con->query($cattle_sql);
                            while($cattle_row = $cattle_result->fetch_assoc()){
                            ?>
                            <td  style="background-color:white;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                            <td style="background-color: #bdbaba  !important;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>
                            <?php } ?>


                            <?php 
                            $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Goat'  AND fit_region='$region' AND fit_province='$province' AND fit_city='$city'";
                            $cattle_result = $con->query($cattle_sql);
                            while($cattle_row = $cattle_result->fetch_assoc()){
                            ?>
                            <td  style="background-color:white;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                            <td style="background-color: #bdbaba  !important;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>
                            <?php } ?>


                            <?php 
                            $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Sheep'  AND fit_region='$region' AND fit_province='$province' AND fit_city='$city'";
                            $cattle_result = $con->query($cattle_sql);
                            while($cattle_row = $cattle_result->fetch_assoc()){
                            ?>
                            <td  style="background-color:white;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                            <td style="background-color: #bdbaba  !important;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>
                            <?php } ?>


                            <?php 
                            $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Chicken' AND fit_region='$region' AND fit_province='$province' AND fit_city='$city'";
                            $cattle_result = $con->query($cattle_sql);
                            while($cattle_row = $cattle_result->fetch_assoc()){
                            ?>
                            <td  style="background-color:white;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                            <td style="background-color: #bdbaba  !important;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>
                            <?php } ?>



                    </tr>

                    <?php
                }
    }
    else
    {
        ?>
            <td colspan="17" class="text-center" style="color: white;">NO DATA</td>
        <?php
    }
}


function mmir_posms_province_me(){
    global $con;
    $region = $_SESSION['region'];

    $from = $_GET['from'];
    $fromdate = DateTime::createFromFormat('Y-m-d',$from);
    $from_date = $fromdate->format('m/d/Y');


