        $sql    =    "SELECT DISTINCT(pm_table.trimming_cause) as trimming_cause FROM disease_report_tbl INNER JOIN pm_table ON disease_report_tbl.drr_id = pm_table.pm_id";
        $result = mysqli_query($con, $sql); 
        while($row = mysqli_fetch_assoc($result)){

            $sqls    =    "SELECT SUM(pm_table.trimmings_head) as trimmings_head,SUM(pm_table.trimming_weight) as trimming_weights, disease_tbl.disease_description as disease_description FROM disease_tbl LEFT JOIN pm_table ON disease_tbl.d_id = pm_table.trimming_cause WHERE disease_level='Critical' AND pm_table.trimming_cause='".$row['trimming_cause']."' GROUP BY disease_tbl.disease_description";
                    $results = mysqli_query($con, $sqls); 
                    while($rows = mysqli_fetch_assoc($results)){

                    ?>
                    <tr>
                        <td ><?php echo $rows['disease_description'] ?></td>
                        <td class="text-right"><?php echo $rows['trimmings_head'] ?></td>
                        <td class="text-right"><?php echo $rows['trimming_weights'] ?></td>
                    </tr>
                    <?php
                 
    }
    }
}


