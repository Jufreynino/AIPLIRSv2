function add_daily_receiving()
{
    global $con;
    date_default_timezone_set('Asia/Manila');
    $today = date("m/d/Y h:i:sa");
    $date = date("m/d/Y");
    $time = date("h:i:sa");
    
    if(isset($_POST['btn_save_drr']))
    {
        $id =  $_SESSION['me_id'];
        $region =  $_SESSION['me_region_code'];
        $species = mysqli_real_escape_string($con, $_POST['species']);
        $origin = mysqli_real_escape_string($con, $_POST['origin']);
        $category = mysqli_real_escape_string($con, $_POST['category']);
        $date_received = mysqli_real_escape_string($con, $_POST['date_received']);
        $time_received = mysqli_real_escape_string($con, $_POST['time_received']);
        $meat_dealer = mysqli_real_escape_string($con, $_POST['meat_dealer']);
        $batch_number = mysqli_real_escape_string($con, $_POST['batch_number']);
        $province = mysqli_real_escape_string($con, $_POST['province']);
        $city = mysqli_real_escape_string($con, $_POST['city']);
        $barangay = mysqli_real_escape_string($con, $_POST['barangay']);
        $broiler_heads = mysqli_real_escape_string($con, $_POST['broiler_heads']);
        $broiler_weight = mysqli_real_escape_string($con, $_POST['broiler_weight']);
        $culled_layer_heads = mysqli_real_escape_string($con, $_POST['culled_layer_heads']);
        $culled_layer_weight = mysqli_real_escape_string($con, $_POST['culled_layer_weight']);
        $native_heads = mysqli_real_escape_string($con, $_POST['native_heads']);
        $native_weight = mysqli_real_escape_string($con, $_POST['native_weight']);
        $total_heads = mysqli_real_escape_string($con, $_POST['total_heads']);
        $total_weight = mysqli_real_escape_string($con, $_POST['total_weight']);
        // $total_average = mysqli_real_escape_string($con, $_POST['total_average']);

        

        $veterenary_name = rand(1000,10000)."-".$_FILES["veterenary"]["name"];
        $veterenary_tname = $_FILES["veterenary"]["tmp_name"];
        $veterenary_uploads_dir = '../../global_assets/images';

        move_uploaded_file($veterenary_tname, $veterenary_uploads_dir.'/'.$veterenary_name);


        $shipping_name = rand(1000,10000)."-".$_FILES["shipping"]["name"];
        $shipping_tname = $_FILES["shipping"]["tmp_name"];
        $shipping_uploads_dir = '../../global_assets/images';

        move_uploaded_file($shipping_tname, $shipping_uploads_dir.'/'.$shipping_name);








        $sql = mysqli_query($con, "INSERT INTO ddr_table (drr_date_time, drr_species, drr_category, drr_meat_dealer, drr_farm_origin, drr_region_code,drr_province_code, drr_city_code, drr_barangay_code, drr_boiler_head, drr_broiler_weight, drr_culled_head, drr_culled_weight, drr_native_head, drr_native_weight, drr_total_head, drr_total_weight,drr_time, drr_date, me_id)
                                                 
                                                                        VALUES('$today','$species','$category','$meat_dealer','$origin','$region','$province','$city','$barangay','$broiler_heads','$broiler_weight','$culled_layer_heads','$culled_layer_weight','$native_heads','$native_weight','$total_heads','$total_weight','$time', '$date','$id')");
        $last_id = $con->insert_id;
        $sql = mysqli_query($con, "INSERT INTO fhc_table (drr_id, fhc_date, fhc_time)
        VALUES('$last_id','$date','$time')");


        $sql = mysqli_query($con, "INSERT INTO fit_human_consumption (drr_id, fit_date, fit_time, fit_species,fit_region, fit_province, fit_city)
        VALUES('$last_id','$date','$time','$species','$region','$province','$city')");

        $sql = mysqli_query($con, "INSERT INTO attachment_tbl (s_permit,drr_id, veterenary_certificate)
        VALUES('$shipping_name','$last_id','$veterenary_name')");
}

} 

function me_per_reg()
{

    global $con;

    $reg = $_GET['reg'];
    $sql = "SELECT * FROM meat_establishment_tbl WHERE me_region_code='$reg' ORDER BY me_id DESC";
    $result = $con->query($sql);
    while($row = $result->fetch_assoc()){
        ?>
        <tr>
            <td style=""><b><?php echo $row['me_plant_name'] ?></b></td>
            <td><?php echo $row['me_type'] ?></td>
            <td><?php echo $row['me_classification'] ?></td>
            <td><?php echo $row['me_lto_number'] ?></td>
            
            <td><?php echo $row['me_expiry'] ?></td>
            <td><?php echo $row['me_region_code'] ?></td>
            <td><a href="me_transaction.php?reg=<?php echo $reg ?>&&mid=<?php echo $row['me_id'] ?>&&mname=<?php echo $row['me_plant_name'] ?>" class="btn btn-primary">View Details</a></td>
        </tr>
     <?php
    }
}


function me_per_transaction_posms()
{
    global $con;

    $reg = $_SESSION['region'];
    date_default_timezone_set('Asia/Manila');
    $today = date("m/d/Y h:i:sa");
    $date = date("m/d/Y");
    $time = date("h:i:sa");

    $sql = "SELECT DISTINCT(meat_establishment_tbl.me_plant_name) as me_plant_name, meat_establishment_tbl.me_type as me_type, meat_establishment_tbl.me_lto_number  as me_lto_number, meat_establishment_tbl.me_region_code as me_region_code, meat_establishment_tbl.me_id as me_id  FROM ddr_table INNER JOIN meat_establishment_tbl ON  ddr_table.me_id =meat_establishment_tbl.me_id  WHERE meat_establishment_tbl.me_region_code='$reg' AND ddr_table.drr_date='$date' AND ddr_table.drr_status='1' ";
    $result = $con->query($sql);
    while($row = $result->fetch_assoc()){
    ?>


        <tr>
            <td><?php echo $row['me_plant_name'] ?></td>
            <td><?php echo $row['me_type'] ?></td>
            <td><?php echo $row['me_lto_number'] ?></td>
            <td><?php echo $date ?></td>
            <td><?php echo $row['me_region_code'] ?></td>
            <td style="width:100px;">
                <a href="dmir.php?md=<?php echo $row['me_id'] ?>&mdate=<?php echo $date  ?>" class="btn btn-primary btn-sm">View</a>
            </td>
        </tr>
    <?php
    }


}


function me_per_reg_posms()
{

    global $con;

    $reg = $_SESSION['region'];
    $sql = "SELECT me_status,me_plant_name,me_type,me_lto_number,me_expiry,me_classification,me_id,me_address,me_barangay_code,me_region_code,me_province_code,me_city_code,me_contact,me_email,me_date_created,employee_id,me_account_activation,case 
    when me_status = 'Pending'
       then 1
    when me_status = 'Approved'
     then 2 
   else 3
   end as status
    FROM meat_establishment_tbl  WHERE me_region_code='$reg' AND me_account_activation='1' ORDER BY status ASC";
    $result = $con->query($sql);
    while($row = $result->fetch_assoc()){
        ?>
        <tr>
            <td><?php echo $row['me_plant_name'] ?></td>

            <td><?php echo $row['me_type'] ?></td>
            <td><?php echo $row['me_classification'] ?></td>

            <td><?php echo $row['me_lto_number'] ?></td>
            <td><?php echo $row['me_expiry'] ?></td>
           
            <?php if($row['status'] == 1){ ?>
                <td><span class="badge bg-warning"> Pending</span></td>   

            <?php }elseif($row['status'] == 2) { ?>
                <td><span class="badge bg-success"> Approved</span></td>   
            
            <?php }else{ ?>
                <td><span class="badge bg-danger"> Rejected</span></td>   

            <?php } ?>


   

            <?php if($row['employee_id'] == 0){ ?>
