                            <td style="background-color: #bdbaba  !important;" class="text-right"><?php echo number_format($hog_row['hog_fit_weight']); ?></td>
                            <?php } ?>



                          <?php 
                            $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Horse'  AND fit_region='$region'";
                            $cattle_result = $con->query($cattle_sql);
                            while($cattle_row = $cattle_result->fetch_assoc()){
                            ?>
                            <td  style="background-color:white;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                            <td style="background-color: #bdbaba  !important;" class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>
                            <?php } ?>


                            <?php 
                            $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE me_id='".$row['me_id']."' AND fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Goat'  AND fit_region='$region'";
                            $cattle_result = $con->query($cattle_sql);
                            while($cattle_row = $cattle_result->fetch_assoc()){
