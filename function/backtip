function count_pending_posms_mio_account()
{
    global $con;
    $region = $_SESSION['region'];
    $result=mysqli_query($con, "SELECT count(*) as posms from employee_tbl WHERE account_status='Pending' AND role='1' AND region='$region' ");
    $data=mysqli_fetch_assoc($result);
    $data['posms'];
    $results=mysqli_query($con, "SELECT count(*) as mio from employee_tbl WHERE account_status='Pending' AND region='$region' AND role='2' ");
    $datas=mysqli_fetch_assoc($results);
    $datas['mio'];
    $sum = $data['posms'] + $datas['mio'];
    if($sum == '0')
    {
    }
    else{
        echo $sum;
    }
}

