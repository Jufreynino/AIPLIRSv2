function activate_email_account_mio()
{
    global $con;
    $id = $_GET['usrid'];

        $sql = mysqli_query($con, "UPDATE employee_tbl SET account_activation='1' WHERE employee_id ='$id' ");
        ?>
        <script type="text/javascript">

            $.jGrowl('Please for the approval to access page', {
                header: 'Successfully Registered',
                theme: 'alert-styled-left alert-arrow-left alert-primary',
                sticky: true,

            });
                
        </script>
        <?php
}

function activate_email_account()
{
    global $con;
    $id = $_GET['me_id'];

        $sql = mysqli_query($con, "UPDATE meat_establishment_tbl SET me_account_activation='1' WHERE me_id ='$id' ");
        ?>
        <script type="text/javascript">

            $.jGrowl('Please for the approval to access page', {
                header: 'Successfully Registered',
                theme: 'alert-styled-left alert-arrow-left alert-primary',
                sticky: true,

            });
                
        </script>
        <?php
}


function resend_activation()
{
    $id = $_SESSION['me_id'];
    $me_email = $_SESSION['me_email'];
    if(isset($_POST['btn_resend_activation']))
    {
         $mail = new PHPMailer(true);

          try{
             $mail->isSMTP();
             $mail->Host = 'smtp.gmail.com';
             $mail->SMTPAuth = true;
             $mail->Username = 'nmis.devops@gmail.com';
             $mail->Password = 'M3@ts@f3ty';
             $mail->SMTPSecure = PHPMailer::ENCRYPTION_STARTTLS;
             $mail->port ='587';
             $mail->setFrom('nmis.devops@gmail.com');
            $mail->AddEmbeddedImage('../../global_assets/images/download.png', 'Kartka');
             $mail->addAddress($me_email);
             $mail->isHTML(true);
             $mail->Subject ='AIPLIRS Notification';
             $mail->Body ='
            

<div style=" margin:auto; width:600px;">
<div style="border:2px solid #c1c1c1; padding:50px; border-radius:15px;">

<center>

                <img src="cid:Kartka">

<h1>Good day!</h1>
</center>

                <p style="text-indent: 100px;">To activate your email, please click the button below  <br><br>
                <center>
                 <a href="http://172.16.8.24/AIPLIRSnative/pages/auth/login_activated.php?me_id='.$id.'" target="_blank" style="font-size:25px; background-color:#0c69f5; color:white; text-decoration:none; padding:10px; border-radius:5px;">Click Here</a>
                </center>
                
                <br><br><br>
                <h4>Department of Agriculture</h4>
                <h3 style="margin-top:-10px;">National Meat Inspection Service</h3>
                <p style="margin-top:-10px;">Plant Operation Standards and Monitoring Division ( POSMD )</p>
            </div>
   </div>';


             $mail->send();
          } catch(Exception $e)
          {

          }
    }

}
function add_meat_establishment()
{
    global $con;
    date_default_timezone_set('Asia/Manila');
    $todays_date = date("m/d/Y h:i:sa");
    $today = strtotime($todays_date);
    
    if(isset($_POST['btn_add_meat_establishment']))
    {
        $code = mysqli_real_escape_string($con, $_POST['code']);
        $plant_name = mysqli_real_escape_string($con, $_POST['plant_name']);
        $type = mysqli_real_escape_string($con, $_POST['type']);
        $lto_number = mysqli_real_escape_string($con, $_POST['lto_number']);
        $region = mysqli_real_escape_string($con, $_POST['region']);
        $city = mysqli_real_escape_string($con, $_POST['city']);
        $barangay = mysqli_real_escape_string($con, $_POST['barangay']);
        $address = mysqli_real_escape_string($con, $_POST['address']);
        $expiration_date = mysqli_real_escape_string($con, $_POST['expiration_date']);
        

        $sql = mysqli_query($con, "INSERT INTO meat_establishment_tbl (me_code,me_plant_name,me_type,me_lto_number,me_region_code,me_city_code,me_barangay_code,me_address,me_status,me_date_created,me_expiry)
        VALUES('$code','$plant_name','$type','$lto_number','$region','$city','$barangay','$address','Active','$todays_date','$expiration_date')");

    }

}  


function meat_establishment_select()
{
    global $con;

    $sql = "SELECT * FROM meat_establishment_tbl WHERE me_status='Approved'";
    $result = $con->query($sql);
                             
    ?><select class="custom-select js-example-basic-single" name="md" required> 
            <option value="">Choose Meat Establishment</option>

    <?php 
    while($row = $result->fetch_assoc()){
    ?>
            <option value="<?php echo $row['me_id'] ?>"><?php echo $row['me_plant_name'] ?></option>

    <?php 
    }
                            
   ?> </select> <?php
}





function fetch_antemortem_disease()
{
    global $con;

    $species = $_GET['species'];
    $category = $_GET['category'];

    $sql = "SELECT * FROM disease_tbl WHERE disease_species='$species' AND disease_type='$category' AND disease_status='Active' AND disease_category='Antemortem'";
    $result = $con->query($sql);
                             
    ?><select class="custom-select js-example-basic-single form-control " style="width: 100% !important;" name="cause" required> 
        <option value="">Choose disease</option>

    <?php 
    while($row = $result->fetch_assoc()){
    ?>
        <option value="<?php echo $row['d_id'] ?>"><?php echo $row['disease_description'] ?></option>
    <?php 
    }
                            
   ?> </select> <?php
}





