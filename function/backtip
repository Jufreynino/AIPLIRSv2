function view_mmir()
{
    global $con;
    $from = $_GET['from'];
    $fromdate = DateTime::createFromFormat('Y-m-d',$from);
    $from_date = $fromdate->format('m/d/Y');


    $to = $_GET['to'];
    $todate = DateTime::createFromFormat('Y-m-d',$to);
    $to_date = $todate->format('m/d/Y');



    ?>
                    <div class="col-md-4 col-sm-6 col-12">
                        <div class="card"  style="background-color:#263238; border-radius: 0px; ">
                            <div class="card-content">
                                <div class="card-body">
                                    <?php

                                    $sql    =    "SELECT SUM(drr_total_head) AS drr_total_head,SUM(drr_total_weight) AS drr_total_weight, drr_species FROM ddr_table  WHERE drr_date BETWEEN '$from_date' AND '$to_date' AND me_id='".$_GET['md']."' GROUP BY drr_species";
                                    $result = mysqli_query($con, $sql); 
                                    while($rows = mysqli_fetch_assoc($result)){ 
                                    ?>
                                            <h3 style="margin-top:-10px;"><?php echo $rows['drr_species'] ?></h3> 

                                            <table border="1">
                                                <tr >
                                                    <th style="font-size: 10px !important;"></th>
                                                    <th style="font-size: 10px !important;">No. of Heads</th>
                                                    <th style="font-size: 10px !important;">Weight</th>
                                                    
                                                </tr>
                                                <tr>
                                                    <th style="font-size: 10px !important;">Received Animals</th>
                                                    <td><?php echo $rows['drr_total_head'] ?></td>
                                                    <td><?php echo $rows['drr_total_weight'] ?></td>
                                                </tr>

                                    <?php

                                    $sql    =    "SELECT SUM(fhc_number_of_heads) AS fhc_number_of_heads,SUM(fhc_weight) AS fhc_weight FROM fhc_table  WHERE fhc_date BETWEEN '$from_date' AND '$to_date' AND me_id='".$_GET['md']."' ";
                                    $result = mysqli_query($con, $sql); 
                                    while($row = mysqli_fetch_assoc($result)){ 
                                    ?>
                                                <tr>
                                                    <th style="font-size: 10px !important;">Fit for Slaughter</th>
                                                    <td><?php echo $row['fhc_number_of_heads'] ?></td>
                                                    <td><?php echo $row['fhc_weight'] ?></td>
                                                </tr>
                                    <?php }  ?>

                                    <?php
                                      $sql    =    "SELECT SUM(fit_number_of_head) AS fit_number_of_head,SUM(fit_weight) AS fit_weight FROM fit_human_consumption  WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND me_id='".$_GET['md']."' ";
                                    $result = mysqli_query($con, $sql); 
                                    while($row = mysqli_fetch_assoc($result)){ 
                                    ?>
                                                <tr>
                                                    <th style="font-size: 10px !important;">Fit for Human Consumption</th>
                                                    <td><?php echo $row['fit_number_of_head'] ?></td>
                                                    <td><?php echo $row['fit_weight'] ?></td>

                                                </tr>
                                    <?php }  ?>
                                            </table>



                                            <form id="myform"  method="POST"  class="form_statusinput">
                                                <input type="hidden"  name="species" id="species" value="<?php echo $rows['drr_species'] ?>">
                                                <input type="hidden"  name="from" id="from" value="<?php echo $_GET['from'] ?>">
                                                <input type="hidden"  name="to" id="to" value="<?php echo $_GET['to'] ?>">
                                                <input type="hidden"  name="md" id="md" value="<?php echo $_GET['md'] ?>">
                                                <div class="  text-right">
                                                    <br>
                                                 <input type="submit" id="button" class="btn bg-teal" value="View Details">
                                                </div>
                                            </form>
                                           
                                </div>
                            </div>
                        </div>
                                    <?php }  ?>
                    </div>
    <?php
}

function count_critical_level()
{
    global $con;
    date_default_timezone_set('Asia/Manila');
    $today = date("m/d/Y h:i:sa");
    $date = date("m/d/Y");
    $time = date("h:i:sa");

    $sql    =    "SELECT count(*) AS critical FROM disease_report_tbl LEFT JOIN disease_tbl ON disease_report_tbl.d_id = disease_tbl.d_id WHERE disease_tbl.disease_level='Critical' AND disease_report_tbl.dr_date='$date'";
    $result = mysqli_query($con, $sql); 
    while($row = mysqli_fetch_assoc($result)){
        ?>
        <td><?php echo $row['critical'] ?></td>
        <?php
    }
}

function count_mid_level()
{
    global $con;
      date_default_timezone_set('Asia/Manila');
    $today = date("m/d/Y h:i:sa");
    $date = date("m/d/Y");
    $time = date("h:i:sa");
    $sql    =    "SELECT count(*) AS critical FROM disease_report_tbl LEFT JOIN disease_tbl ON disease_report_tbl.d_id = disease_tbl.d_id WHERE disease_tbl.disease_level='Mid'  AND disease_report_tbl.dr_date='$date'";
    $result = mysqli_query($con, $sql); 
    while($row = mysqli_fetch_assoc($result)){
        ?>
        <td><?php echo $row['critical'] ?></td>
        <?php
    }
}


function count_low_level()
{
    global $con;
    date_default_timezone_set('Asia/Manila');
    $today = date("m/d/Y h:i:sa");
    $date = date("m/d/Y");
    $time = date("h:i:sa");
    $sql    =    "SELECT count(*) AS critical FROM disease_report_tbl LEFT JOIN disease_tbl ON disease_report_tbl.d_id = disease_tbl.d_id WHERE disease_tbl.disease_level='Low'  AND disease_report_tbl.dr_date='$date'";
    $result = mysqli_query($con, $sql); 
    while($row = mysqli_fetch_assoc($result)){
        ?>
        <td><?php echo $row['critical'] ?></td>
        <?php
    }
}




function fetch_all_critical_postmortem_carcass()
{
    global $con;
        $sql    =    "SELECT DISTINCT(pm_table.carcass_cause) as carcass_cause FROM disease_report_tbl INNER JOIN pm_table ON disease_report_tbl.drr_id = pm_table.pm_id";
        $result = mysqli_query($con, $sql); 
        while($row = mysqli_fetch_assoc($result)){

            $sqls    =    "SELECT SUM(pm_table.carcass_number_of_heads) as carcass_number_of_heads,SUM(pm_table.carcass_weight) as carcass_weights, disease_tbl.disease_description as disease_description FROM disease_tbl LEFT JOIN pm_table ON disease_tbl.d_id = pm_table.carcass_cause WHERE disease_level='Critical' AND pm_table.carcass_cause='".$row['carcass_cause']."' GROUP BY disease_tbl.disease_description";
                    $results = mysqli_query($con, $sqls); 
                    while($rows = mysqli_fetch_assoc($results)){

                    ?>
                    <tr>
                        <td ><?php echo $rows['disease_description'] ?></td>
                        <td class="text-right"><?php echo $rows['carcass_number_of_heads'] ?></td>
                        <td class="text-right"><?php echo $rows['carcass_weights'] ?></td>
                    </tr>
                    <?php
                 
    }
    }
}


function fetch_all_low_postmortem_carcass()
{
    global $con;
        $sql    =    "SELECT DISTINCT(pm_table.carcass_cause) as carcass_cause FROM disease_report_tbl INNER JOIN pm_table ON disease_report_tbl.drr_id = pm_table.pm_id";
        $result = mysqli_query($con, $sql); 
        while($row = mysqli_fetch_assoc($result)){

            $sqls    =    "SELECT SUM(pm_table.carcass_number_of_heads) as carcass_number_of_heads,SUM(pm_table.carcass_weight) as carcass_weights, disease_tbl.disease_description as disease_description FROM disease_tbl LEFT JOIN pm_table ON disease_tbl.d_id = pm_table.carcass_cause WHERE disease_level='Low' AND pm_table.carcass_cause='".$row['carcass_cause']."' GROUP BY disease_tbl.disease_description";
                    $results = mysqli_query($con, $sqls); 
                    while($rows = mysqli_fetch_assoc($results)){

                    ?>
                    <tr>
                        <td ><?php echo $rows['disease_description'] ?></td>
                        <td class="text-right"><?php echo $rows['carcass_number_of_heads'] ?></td>
                        <td class="text-right"><?php echo $rows['carcass_weights'] ?></td>
                    </tr>
                    <?php
                 
    }
    }
}

