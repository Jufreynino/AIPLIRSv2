function count_chicken_slaughtered_region_weight()
{
    
    global $con;
    date_default_timezone_set('Asia/Manila');
    $date = date("m/d/Y");
    $region = $_SESSION['region'];
    $plant_name = $_SESSION['plant_name'];
    $result=mysqli_query($con, "SELECT SUM(fit_weight) as total from fit_human_consumption INNER JOIN ddr_table ON ddr_table.drr_id =fit_human_consumption.drr_id  WHERE ddr_table.me_id='$plant_name' AND fit_human_consumption.fit_species='Chicken' AND fit_human_consumption.fit_date='$date' ");
    $data=mysqli_fetch_assoc($result);
    if($data['total'] =='0'){
        echo '0';
    }
    else{
        echo $data['total'];

    }
}

