function count_monthly_receive_fit_for_slaughter_heads()
{
 
    global $con;
    $md = $_GET['meat_establishment'];
    $plant_name = $_SESSION['plant_name'];
    $from = $_GET['from'];
    $fromdate = DateTime::createFromFormat('Y-m-d',$from);
    $from_date = $fromdate->format('m/d/Y');


    $to = $_GET['to'];
    $todate = DateTime::createFromFormat('Y-m-d',$to);
    $to_date = $todate->format('m/d/Y');


    $result = mysqli_query($con, "SELECT SUM(fhc_table.fhc_number_of_heads) as total FROM fhc_table INNER JOIN ddr_table on  ddr_table.drr_id = fhc_table.drr_id  WHERE ddr_table.drr_date BETWEEN '$from_date' AND '$to_date'  AND fhc_table.me_id='$md'");
    $data=mysqli_fetch_assoc($result);

    if($data['total'] =='0'){


    }
    else{
        echo number_format($data['total'],2);

    }
}

