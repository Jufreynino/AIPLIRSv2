function modify_antemortem_sum_total_heads()
{
    global $con;
    date_default_timezone_set('Asia/Manila');
   
    $id     =   $_GET['mid'];
    $date     =   $_GET['mdate'];
    $sum = 0;
    $sql = "SELECT am_table.c_heads as c_heads, am_table.r_heads as r_heads, ddr_table.drr_total_head as drr_total_head FROM am_table LEFT JOIN ddr_table on am_table.drr_id= ddr_table.drr_id WHERE am_table.drr_id='$id' AND ddr_table.drr_date='$date '";
    $result = $con->query($sql);

    if ($result->num_rows > 0) {
        while($row = $result->fetch_assoc()) {
            
            $sum += floatval($row['c_heads']) + floatval($row['r_heads']);

            $total =  $row['drr_total_head'] - $sum;
    }
            echo number_format($total);
            ?>
             <input type="hidden" name="total_of_heads" id="total_number_of_heads_received" value="<?php echo $total?>">
            <?php
    }
    else{
            $sql = "SELECT * FROM ddr_table  WHERE drr_id='$id' AND drr_date='$date'";
            $result = $con->query($sql);
            $row = $result->fetch_assoc();
            $totalsss =  $row['drr_total_head'];
            echo number_format($totalsss);
            ?>
            <input type="hidden" name="total_of_heads" id="total_number_of_heads_received" value="<?php echo $totalsss?>">
           <?php
    }
}


function modify_postmortem_sum_total_weight()
{
    
    global $con;

    $id = $_GET['mid'];
    $date = $_GET['mdate'];
    $sql = "SELECT pm_table.carcass_weight as carcass_weight , fhc_table.fhc_weight as fhc_weight, fhc_table.fhc_number_of_heads as fhc_number_of_heads, fhc_table.fhc_date as fhc_date FROM pm_table LEFT JOIN fhc_table on pm_table.drr_id= fhc_table.drr_id WHERE pm_table.drr_id='$id' AND pm_table.pm_date='$date'";
    $result = $con->query($sql);
    $row = $result->fetch_assoc();
    $sum = 0;


    if ($result->num_rows > 0) {
        while($row = $result->fetch_assoc()) {
    
            $sum += $row['carcass_weight'];

            $total =  $row['fhc_weight'] - $sum;
            $dates = $row['fhc_date'];
    }
            echo number_format($total,2);

            ?>
             <input type="hidden" name="total_of_weight" id="total_of_weight" value="<?php echo $total?>">
             <input type="hidden" name="date" value="<?php echo $dates ?>">
            <?php
    }
    else{
            $sqls = "SELECT * FROM fhc_table  WHERE drr_id='$id' AND fhc_date='$date'";
            $results = $con->query($sqls);
            $rows = $results->fetch_assoc();
             echo number_format($rows['fhc_weight'],2);
             
            ?>
            
             <input type="hidden" name="total_of_weight" id="total_of_weight" value="<?php echo $row['fhc_weight']?>">
             <input type="hidden" name="date" value="<?php echo $date ?>">


           <?php
    }

      
}


function modify_postmortem_sum_total_heads()
{

    

    global $con;
  
    $id     =   $_GET['mid'];
    $date     =   $_GET['mdate'];
    $sum = 0;

    $sql = "SELECT pm_table.carcass_number_of_heads as carcass_number_of_heads ,pm_table.lung_weight as lung_weight ,pm_table.liver_weight as liver_weight ,pm_table.heart_weight as heart_weight ,pm_table.intestine_weight as intestine_weight, pm_table.feet_weight as feet_weight, pm_table.kidney_weight as kidney_weight, pm_table.spleen_weight as spleen_weight, fhc_table.fhc_weight as fhc_weight, fhc_table.fhc_number_of_heads as fhc_number_of_heads, fhc_table.fhc_date as fhc_date FROM pm_table LEFT JOIN fhc_table on pm_table.drr_id= fhc_table.drr_id WHERE pm_table.drr_id='$id' AND pm_table.pm_date='$date'";


    $result = $con->query($sql);

    if ($result->num_rows > 0) {
        while($row = $result->fetch_assoc()) {
    
            $sum += $row['carcass_number_of_heads'];

            $total =  $row['fhc_number_of_heads'] - $sum;
            $dates = $row['fhc_date'];
    }
            echo $total;

            ?>
             <input type="hidden" name="total_of_heads" id="total_of_heads" value="<?php echo $total?>">
             <input type="hidden" name="date" value="<?php echo $dates ?>">
            <?php
    }
    else{
            $sqls = "SELECT * FROM fhc_table  WHERE drr_id='$id' AND fhc_date='$date'";
            $results = $con->query($sqls);
            $rows = $results->fetch_assoc();
             echo $rows['fhc_number_of_heads'];
             
            ?>
            <input type="hidden" name="total_of_heads" id="total_of_heads" value="<?php echo $totalsss?>">
             <input type="hidden" name="date" value="<?php echo $row['fhc_date']?>">


           <?php
    }
       
        
      
}







function udpate_fhc_daily()
{
    global $con;
   if(isset($_POST['btn_save_fhc']))
   {
        $id = $_GET['mid'];
        $med = $_GET['med'];
        $date = $_GET['mdate'];
        $drr_total_head  = floatval($_POST['total_of_heads']);
        $total_of_weight  = floatval($_POST['total_of_weight']);
        $me_id = $_GET['med'];


        $sql = mysqli_query($con, "UPDATE fhc_table SET fhc_number_of_heads='$drr_total_head', fhc_weight='$total_of_weight', me_id='$me_id' WHERE drr_id='$id' AND fhc_date='$date'");

        ?>
            <script type="text/javascript">
                window.location = "postmortem.php?med=<?php echo $_GET['med'] ?>&mid=<?php echo $id  ?>&mdate=<?php echo $date ?>&species=<?php echo $_GET['species']?>&category=<?php echo $_GET['category']?>";
            </script>
        <?php

   }
}

function udpate_fit_daily()
{
    global $con;
  
   if(isset($_POST['btn_save_fit']))
   {
        $id = $_GET['mid'];
        $date = $_GET['mdate'];
        $drr_total_head  = $_POST['total_of_heads'];
        $total_of_weight  = $_POST['total_of_weight'];
        $dates  = $_POST['date'];
        $me_id = $_GET['med'];

        $sql = mysqli_query($con, "UPDATE fit_human_consumption SET fit_number_of_head='$drr_total_head', fit_weight='$total_of_weight', me_id='$me_id' WHERE drr_id='$id' AND fit_date='$date'");

        $sql = mysqli_query($con, "UPDATE ddr_table SET drr_status='1' WHERE drr_id='$id' AND drr_date='$date'");


        ?>
            <script type="text/javascript">
                window.location = "am_pm_list.php?md=<?php echo $_GET['med']  ?>&mdate=<?php echo $dates ?>";
            </script>
        <?php

   }
}


