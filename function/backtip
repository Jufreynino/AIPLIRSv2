function mmir_posms_city(){
    global $con;
    $region = $_SESSION['region'];

    $from = $_GET['from'];
    $fromdate = DateTime::createFromFormat('Y-m-d',$from);
    $from_date = $fromdate->format('m/d/Y');


    $to = $_GET['to'];
    $todate = DateTime::createFromFormat('Y-m-d',$to);
    $to_date = $todate->format('m/d/Y');

    $province = $_GET['province'];
    $city = $_GET['city'];

    $sql = "SELECT * FROM fit_human_consumption WHERE fit_province='$province' AND fit_city='$city' AND fit_date BETWEEN '$from_date' AND '$to_date' GROUP BY fit_city";
    $result = $con->query($sql);

    if(mysqli_num_rows($result) > 0)
    {
            while($row = $result->fetch_assoc())
            {
                ?>
                <tr>
                       <td   class="sticky-col first-col"><?php echo $row['fit_city']; ?></td>



                        <?php 

                        $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Cattle' AND fit_region='$region' AND fit_province='$province' AND fit_city='$city'";
                        $cattle_result = $con->query($cattle_sql);
                        while($cattle_row = $cattle_result->fetch_assoc()){

                        ?>
                        <td  style="background-color: white !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                        <td  style="background-color: #bdbaba  !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>



                        <?php } ?>

                         <?php 

                        $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Carabao' AND fit_region='$region' AND fit_province='$province' AND fit_city='$city'";
                        $cattle_result = $con->query($cattle_sql);
                        while($cattle_row = $cattle_result->fetch_assoc()){

                        ?>
                        <td  style="background-color: white !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                        <td  style="background-color: #bdbaba  !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>

                        <?php } ?>



                        <?php 
                        $hog_sql = "SELECT SUM(fit_number_of_head) as hog_fit_number_of_head, SUM(fit_weight) as hog_fit_weight  FROM fit_human_consumption  WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Hog' AND fit_region='$region' AND fit_province='$province' AND fit_city='$city'";




                        $hog_result = $con->query($hog_sql);
                        while($hog_row = $hog_result->fetch_assoc()){
                        ?>
                        <td  style="background-color: white !important;"  class="text-right"><?php echo number_format($hog_row['hog_fit_number_of_head']); ?></td>
                        <td  style="background-color: #bdbaba  !important;"  class="text-right"><?php echo number_format($hog_row['hog_fit_weight']); ?></td>
                        <?php } ?>



                      <?php 
                        $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Horse'  AND fit_region='$region' AND fit_province='$province' AND fit_city='$city'";
                        $cattle_result = $con->query($cattle_sql);
                        while($cattle_row = $cattle_result->fetch_assoc()){
                        ?>
                        <td  style="background-color: white !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                        <td style="background-color: #bdbaba  !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>
                        <?php } ?>


                        <?php 
                        $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Goat' AND fit_region='$region' AND fit_province='$province' AND fit_city='$city'";
                        $cattle_result = $con->query($cattle_sql);
                        while($cattle_row = $cattle_result->fetch_assoc()){
                        ?>
                        <td style="background-color: white !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                        <td style="background-color: #bdbaba  !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>
                        <?php } ?>


                        <?php 
                        $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='sheep ' AND fit_region='$region' AND fit_province='$province' AND fit_city='$city'";
                        $cattle_result = $con->query($cattle_sql);
                        while($cattle_row = $cattle_result->fetch_assoc()){
                        ?>
                        <td style="background-color: white !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                        <td style="background-color: #bdbaba  !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>
                        <?php } ?>


                        <?php 
                        $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Chicken' AND fit_region='$region' AND fit_province='$province' AND fit_city='$city'";
                        $cattle_result = $con->query($cattle_sql);
                        while($cattle_row = $cattle_result->fetch_assoc()){
                        ?>
                        <td style="background-color: white !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                        <td style="background-color: #bdbaba  !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>
                        <?php } ?>
                </tr>

                <?php
                }
                }
        else{
            ?>
                <td colspan="17" class="text-center" style="color: white;">NO DATA</td>
            <?php 
        }

}




function mmir_posmd_city(){
    global $con;
    $region = $_SESSION['region'];

    $from = $_GET['from'];
    $fromdate = DateTime::createFromFormat('Y-m-d',$from);
    $from_date = $fromdate->format('m/d/Y');


    $to = $_GET['to'];
    $todate = DateTime::createFromFormat('Y-m-d',$to);
    $to_date = $todate->format('m/d/Y');

    $province = $_GET['province'];
    $city = $_GET['city'];

    $sql = "SELECT * FROM fit_human_consumption WHERE fit_province='$province' AND fit_city='$city' AND fit_date BETWEEN '$from_date' AND '$to_date' GROUP BY fit_city";
    $result = $con->query($sql);

    if(mysqli_num_rows($result) > 0)
    {
            while($row = $result->fetch_assoc())
            {
                ?>
                <tr>
                       <td   class="sticky-col first-col"><?php echo $row['fit_city']; ?></td>

                       <?php 
                        $hog_sql = "SELECT SUM(fit_number_of_head) as hog_fit_number_of_head, SUM(fit_weight) as hog_fit_weight  FROM fit_human_consumption  WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Hog' AND fit_province='$province' AND fit_city='$city'";
                        $hog_result = $con->query($hog_sql);
                        while($hog_row = $hog_result->fetch_assoc()){
                        ?>
                        <td  style="background-color: white !important;"  class="text-right"><?php echo number_format($hog_row['hog_fit_number_of_head']); ?></td>
                        <td  style="background-color: #bdbaba !important;"  class="text-right"><?php echo number_format($hog_row['hog_fit_weight']); ?></td>
                        <?php } ?>

                           <?php 
                        $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Chicken' AND fit_province='$province' AND fit_city='$city'";
                        $cattle_result = $con->query($cattle_sql);
                        while($cattle_row = $cattle_result->fetch_assoc()){
                        ?>
                        <td style="background-color: white !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                        <td style="background-color: #bdbaba !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>
                        <?php } ?>

                        <?php 

                        $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Cattle' AND fit_province='$province' AND fit_city='$city'";
                        $cattle_result = $con->query($cattle_sql);
                        while($cattle_row = $cattle_result->fetch_assoc()){

                        ?>
                        <td  style="background-color: white !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                        <td  style="background-color: #bdbaba !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>



                        <?php } ?>

                         <?php 

                        $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Carabao' AND fit_province='$province' AND fit_city='$city'";
                        $cattle_result = $con->query($cattle_sql);
                        while($cattle_row = $cattle_result->fetch_assoc()){

                        ?>
                        <td  style="background-color: white !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_number_of_head']); ?></td>
                        <td  style="background-color: #bdbaba !important;"  class="text-right"><?php echo number_format($cattle_row['cattle_fit_weight']); ?></td>

                        <?php } ?>



                    



                      <?php 
                        $cattle_sql = "SELECT SUM(fit_number_of_head) as cattle_fit_number_of_head, SUM(fit_weight) as cattle_fit_weight  FROM fit_human_consumption  WHERE fit_date BETWEEN '$from_date' AND '$to_date' AND fit_species='Horse'  AND fit_province='$province' AND fit_city='$city'";
