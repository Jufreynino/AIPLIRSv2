        </tr>
     <?php
 }

     if(isset($_POST['assign_mio_posms']))
     {
        $me_id = $_POST['me_id'];
        $mio_id = $_POST['mio_id'];
        $sql = mysqli_query($con, "UPDATE meat_establishment_tbl SET employee_id='$mio_id' WHERE me_id='$me_id' ");
        $update_mio = mysqli_query($con, "UPDATE employee_tbl  SET assign='1' WHERE employee_id='$mio_id' ");
        ?>
            <script type="text/javascript">

            $.jGrowl('Please for the approval to access page', {
                header: 'Successfully Registered',
                theme: 'alert-styled-left alert-arrow-left alert-primary',
                sticky: true,

            });

                 setTimeout(function () {
                      window.location = "me_per_region.php";
                    }, 2000); 
                
            </script>
        <?php

     }
}


function update_application_information()
{

    global $con;

    $sql = "SELECT * FROM meat_establishment_tbl WHERE me_id='".$_SESSION['me_id']."'";
    $result = $con->query($sql);
   $row = $result->fetch_assoc();

    ?>
    <form method="POST" class="flex-fill">
                        <?php if(isset($error_classification)){echo $error_classification; }?>
