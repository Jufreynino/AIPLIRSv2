function me_posmd()
{

    global $con;

    $reg = $_SESSION['region'];
    $sql = "SELECT me_status,me_plant_name,me_type,me_lto_number,me_expiry,me_id,me_address,me_classification,me_province_code,me_barangay_code,me_region_code,me_city_code,me_contact,me_email,me_date_created,employee_id,case 
    when me_status = 'Pending'
       then 1
    when me_status = 'Approved'
     then 2 
   else 3
   end as status
    FROM meat_establishment_tbl WHERE me_account_activation='1' ORDER BY status ASC";
    $result = $con->query($sql);
    while($row = $result->fetch_assoc()){
        ?>
        <tr>
            <td><?php echo $row['me_plant_name'] ?></td>

            <td><?php echo $row['me_type'] ?></td>
            <td><?php echo $row['me_classification'] ?></td>

            
            <td><?php echo $row['me_lto_number'] ?></td>
            <td><?php echo $row['me_expiry'] ?></td>
           
            <?php if($row['status'] == 1){ ?>
                <td><span class="badge bg-warning"> Pending</span></td>   

            <?php }elseif($row['status'] == 2) { ?>
                <td><span class="badge bg-success"> Approved</span></td>   
            
            <?php }else{ ?>
                <td><span class="badge bg-danger"> Rejected</span></td>   

            <?php } ?>


            <?php if($row['employee_id'] == 0){ ?>
                <td style="width: 150px;"><span class="badge bg-warning"> Unassigned</span></td>   

             
            
            <?php }else{ ?>
                <td style="width: 150px;">
                     <?php 
                        $sqls = "SELECT * FROM employee_tbl  WHERE employee_id='".$row['employee_id']."'";
                        $results = $con->query($sqls);
                        $rows = $results->fetch_assoc(); 
                        echo $rows['lastname'].', '.$rows['firstname']
                        ?>
                </td>   
            <?php } ?>
            <td style="width:130px;">
                <button type="button" class="btndmir_mio bg-primary" data-toggle="modal" data-target="#view_more_details<?php echo $row['me_id'] ?>">View Details </button>

                <div id="view_more_details<?php echo $row['me_id'] ?>" class="modal fade" tabindex="-1" data-backdrop="static" aria-hidden="true">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header bg-primary">
                                <h6 class="modal-title">Meat Establishment Details</h6>
                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                            </div>

                            <div class="modal-body">
                                <h3 class="font-weight-semibold text-center"><?php echo $row['me_plant_name'] ?></h3>
                                <h6 class=" text-center" style="margin-top:-10px; font-size: 13px;"> Meat Establishment Name</h6>
                                <br>
                               <div class="row">
                                    <div class="col-md-3  text-center">
                                        <b> <?php echo $row['me_type'] ?> </b>
                                        <br>
                                       <small>Type</small>
                                    </div>

                                     <div class="col-md-3  text-center">
                                        <b> <?php echo $row['me_classification'] ?> </b>
                                        <br>
                                       <small>Classification</small>
                                    </div>


                                     <div class="col-md-3  text-center">
                                        <b> <?php echo $row['me_lto_number'] ?> </b>
                                        <br>
                                       <small>LTO Number</small>
                                    </div>

                                    <div class="col-md-3  text-center">
                                        <b> <?php echo $row['me_expiry'] ?> </b>
                                        <br>
                                       <small>LTO Expiration date</small>
                                    </div>
                                </div>
                                <br>

                                <legend><b>Contact Details</b></legend>
                                 <div class="row ">
                                    <div class="col-md-6">
                                        <b> <?php echo $row['me_contact'] ?> </b>
                                        <br>
                                       <small>Contact #</small>
                                    </div>

                                     <div class="col-md-6">
                                        <b> <?php echo $row['me_email'] ?> </b>
                                        <br>
                                       <small>Email</small>
                                    </div>
                                </div>
                                <br>


                                 <legend><b>Meat Inspection Officer Assigned</b></legend>
                                     <div class="row text-center">
                                        <div class="col-md-12">
                                    <b>
                                            <?php if($row['employee_id'] == 0){ ?>
                                                    No Meat Inspection Officer Assigned
             
            
                                                <?php }else{ ?>
                                                         <?php 
                                                            $sqls = "SELECT * FROM employee_tbl  WHERE employee_id='".$row['employee_id']."'";
                                                            $results = $con->query($sqls);
                                                            $rows = $results->fetch_assoc(); 
                                                            echo $rows['lastname'].', '.$rows['firstname']
                                                            ?>
                                                <?php } ?>


                                              </b>
                                            <br>
                                           <small>Meat Inspection Officer</small>
                                        </div>
                                    </div>


                                <legend><b>Address Details</b></legend>
                                     <div class="row">
                                        
                                        
