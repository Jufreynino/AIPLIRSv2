    $md = $_GET['md'];
    $mdate = $_GET['mdate'];
    $plant_name = $_SESSION['plant_name'];



    $result = mysqli_query($con, "SELECT SUM(fit_human_consumption.fit_weight) as fit_weight FROM fit_human_consumption INNER JOIN ddr_table on  ddr_table.drr_id = fit_human_consumption.drr_id WHERE fit_human_consumption.fit_date='$mdate' ORDER BY fit_time DESC");
    $data=mysqli_fetch_assoc($result);

    if($data['fit_weight'] =='0'){


    }
    else{
        echo number_format($data['fit_weight'],2);

    }
}

function count_daily_receive_fit_for_slaughter_weight()
{
 
    global $con;
    $md = $_GET['md'];
    $mdate = $_GET['mdate'];
    $plant_name = $_SESSION['plant_name'];



    $result = mysqli_query($con, "SELECT SUM(fhc_table.fhc_weight) as total FROM fhc_table INNER JOIN ddr_table on  ddr_table.drr_id = fhc_table.drr_id  WHERE  fhc_table.fhc_date='$mdate' AND fhc_table.me_id='$md' AND ddr_table.drr_status='1'  ORDER BY fhc_time DESC");
    $data=mysqli_fetch_assoc($result);

    if($data['total'] =='0'){


    }
    else{
        echo number_format($data['total'],2);

    }
}





