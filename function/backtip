function show_owner_me_drr()
{

    $date = date("m/d/Y");
    $id = $_SESSION['me_id'];

    global $con;

    $sql = "SELECT COUNT(drr_date) as number , drr_date, drr_date_time,drr_report_status, drr_inspection_status,drr_id,drr_meat_dealer FROM ddr_table WHERE me_id='$id'  GROUP BY drr_date ORDER BY drr_id DESC ";
    $result = $con->query($sql);

        
    if ($result->num_rows > 0) {
        while($row = $result->fetch_assoc()) {
            ?>
            
            <?php if($row['drr_date'] == $date) { ?>
            <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8; ">
            <?php } else{  ?>
                <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8; background-color:#d3d4d7;">
            <?php } ?>
                <td><b> <?php echo $row['drr_date']  ?></b></td>
                <td><b> <?php echo $row['number']  ?></b></td>
                <td>
                    <a href="am_pm_list.php?md=<?php echo $id ?>&mdate=<?php echo $row['drr_date'] ?>  " class="btn btn-info btn-icon"><i class="icon-file-eye"></i></a>
                </td>
            </tr>
            <?php

        }
    } else {
        ?>
        <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8;">
            <td class="text-center" colspan="6"><b> No Records Found!</b></td>
        </tr>
        <?php
    }
}


function show_md_drr_me()
{

    $date = date("m/d/Y");


    $date_md = $_GET['mdate'];
    $mid = $_GET['md'];

    global $con;

    $sql = "SELECT * FROM ddr_table WHERE drr_date='$date_md' AND me_id='$mid' ORDER BY drr_time DESC ";
    $result = $con->query($sql);



    if ($result->num_rows > 0) {
        
    while($row = $result->fetch_assoc()) {
     
        
        ?>
        <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8;">
            <td><b> <?php echo $row['drr_time'];  ?></b></td>
            <td><b> <?php echo $row['drr_meat_dealer']  ?></b></td>
            <td>
                <?php if($row['drr_status'] == 0) {?>
                    <span class="badge bg-warning">Pending</span>
                <?php } else { ?>
                <span class="badge bg-success">Inspected</span>

                <?php } ?>

            </td>
                <?php if($row['drr_report_status'] == 0){ ?>


                <!-- <button type="button" class=" btn-sm btn bg-teal item-detail" style="color:white;" data="<?php echo $row['drr_id'] ?>">Track</button> -->



                <?php } else { ?>
                <?php } ?>
        </tr>
        <?php

    }
    } else {
        ?>
        <tr style="padding:10px !important; margin:20px !important; border:1px solid #bdb8b8;">
            <td class="text-center" colspan="6"><b> No Records Found!</b></td>
        </tr>
        <?php
    }
}

function disease_report_postmortem_daily()
{

    date_default_timezone_set('Asia/Manila');
    $today_date = date("m/d/Y");
    global $con;
    $sql = "SELECT *, CONCAT(carcass_cause, lung_cause,liver_cause,heart_cause,intestine_cause,trimming_cause,feet_cause,kidney_cause,spleen_cause) AS column_name, SUM(carcass_weight) AS carcass_weight,  SUM(lung_weight) AS lung_weight, SUM(liver_weight) AS liver_weight, 
    SUM(heart_weight) AS heart_weight, 
    SUM(intestine_weight) AS intestine_weight, 
    SUM(trimming_weight) AS trimming_weight, 
    SUM(feet_weight) AS feet_weight, 
    SUM(kidney_weight) AS kidney_weight, 
    SUM(spleen_weight) AS spleen_weight, 


     SUM(carcass_number_of_heads) AS carcass_number_of_heads, 

      SUM(lung_number_of_affected) AS lung_number_of_affected, 


       SUM(liver_number_of_affected) AS liver_number_of_affected, 

        SUM(heart_number_of_affected) AS heart_number_of_affected, 
         SUM(intestine_number_of_affected) AS intestine_number_of_affected, 
          SUM(trimmings_head) AS trimmings_head, 
           SUM(feet_number_of_affected) AS feet_number_of_affected, 
            SUM(kidney_number_of_affected) AS kidney_number_of_affected, 
            SUM(spleen_number_of_affected) AS spleen_number_of_affected, 
    CONCAT(carcass_weight, lung_weight,liver_weight,heart_weight,intestine_weight,trimming_weight,feet_weight,kidney_weight,spleen_weight) AS column_names FROM pm_table GROUP BY column_name";
    $result = $con->query($sql);
    while($row = $result->fetch_assoc()) {

                            $total_weight = $row['carcass_weight'] + $row['lung_weight'] + $row['liver_weight']+ $row['heart_weight']+ $row['intestine_weight']+ $row['trimming_weight']+ $row['feet_weight']+ $row['kidney_weight']+ $row['spleen_weight'];


                            $total_heads = $row['carcass_number_of_heads'] + $row['lung_number_of_affected'] + $row['liver_number_of_affected']+ $row['heart_number_of_affected']+ $row['intestine_number_of_affected']+ $row['trimmings_head']+ $row['feet_number_of_affected']+ $row['kidney_number_of_affected']+ $row['spleen_number_of_affected'];







                                $sqls = "SELECT * FROM disease_tbl WHERE d_id='".$row['column_name']."'";
                                $results = $con->query($sqls);
                                while($rows = $results->fetch_assoc()) {
                ?>
                                    <tr>
                                        <td style="width:150px;"><?php echo $rows['disease_code'] ?></td>
                                        <td style="width:400px;" ><b> <?php echo $rows['disease_description'] ?></b></td>
                                        <td><?php echo date('F d, Y', strtotime($today_date)); ?></td>
                                        <td style="color:blue;"><?php echo number_format($total_heads); ?> Hds</td>

                                        <td style="color:red;"><?php echo number_format($total_weight,2); ?> Kg</td>
                                       <td style="width:130px;"><a href="disease_postmortem_daily_report_me.php?d_id=<?php echo $rows['d_id'] ?>&date=<?php echo $today_date ?>&heads=<?php echo $total_heads ?>&weight=<?php echo $total_weight ?>" class="btn btn-primary" style="color:white;">View Details</a></td>
                                    </tr>
                <?php
        }
    }
}




function disease_report_postmortem_daily_selected_disease_me()
{

    date_default_timezone_set('Asia/Manila');
    $today_date = date("m/d/Y");
    $d_id = $_GET['d_id'];
    $date = $_GET['date'];
    global $con;
    $sql = "SELECT *,
        SUM(pm_table.spleen_number_of_affected + pm_table.kidney_number_of_affected + pm_table.feet_number_of_affected + pm_table.trimmings_head + pm_table.intestine_number_of_affected + pm_table.heart_number_of_affected + pm_table.liver_number_of_affected + pm_table.lung_number_of_affected + pm_table.carcass_number_of_heads ) as total_heads_affected, 


        SUM(pm_table.carcass_weight + pm_table.lung_weight + pm_table.liver_weight + pm_table.heart_weight + pm_table.intestine_weight + pm_table.trimming_weight + pm_table.feet_weight + pm_table.kidney_weight + pm_table.spleen_weight ) as total_weight, 


        meat_establishment_tbl.me_type AS me_type, meat_establishment_tbl.me_classification AS me_classification, meat_establishment_tbl.me_lto_number AS me_lto_number, meat_establishment_tbl.me_region_code AS me_region_code, meat_establishment_tbl.me_province_code AS me_province_code, meat_establishment_tbl.me_city_code AS me_city_code, meat_establishment_tbl.me_barangay_code AS me_barangay_code




    FROM pm_table 
    INNER JOIN ddr_table ON pm_table.drr_id = ddr_table.drr_id

    INNER JOIN meat_establishment_tbl ON pm_table.me_id = meat_establishment_tbl.me_id 
    WHERE carcass_cause='$d_id' 
    OR lung_cause='$d_id' 
    OR liver_cause='$d_id' 
    OR heart_cause='$d_id' 
    OR intestine_cause='$d_id' 
    OR trimming_cause='$d_id' 
    OR feet_cause='$d_id' 
    OR kidney_cause='$d_id'
    OR spleen_cause='$d_id' GROUP BY meat_establishment_tbl.me_plant_name";
    $result = $con->query($sql);
    while($row = $result->fetch_assoc()) {

                            
    $total_heads =$row['total_heads_affected'];
    $total_weight =$row['total_weight'];




                ?>
                                    <tr>
                                        <td style="width:150px;"><?php echo $row['me_plant_name'] ?></td>
                                        <td style="width:150px;"><?php echo $row['me_type'] ?></td>
                                        <td style="width:150px;"><?php echo $row['me_classification'] ?></td>
                                        <td style="width:150px;"><?php echo $row['me_lto_number'] ?></td>
                                        <td style="width:150px;"><?php echo $row['me_region_code'] ?></td>
                                        <td style="width:150px;"><?php echo $row['me_province_code'] ?></td>
                                        <td style="width:150px;"><?php echo $row['me_city_code'] ?></td>
                                        <td style="width:150px;"><?php echo $row['me_barangay_code'] ?></td>
                                        <td style="width:150px; color:blue;"><?php echo number_format($total_heads) ?> Hds</td>
                                        <td style="width:150px; color:red;"><?php echo number_format($total_weight,2) ?> Kg</td>
                                       
                                    </tr>
                <?php
    }
}


function disease_report_postmortem_daily_selected_disease()
{

