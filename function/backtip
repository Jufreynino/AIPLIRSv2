function count_pending_meat_establishment_account()
{
    
    global $con;
    $region = $_SESSION['region'];
    $result=mysqli_query($con, "SELECT count(*) as total from meat_establishment_tbl WHERE me_status='Pending' AND me_region_code='$region' AND me_account_activation='1' ");
    $data=mysqli_fetch_assoc($result);

    if($data['total'] =='0'){


    }
    else{
        echo $data['total'];

    }
}


function count_daily_slaughthered_heads()
{
    global $con;
    date_default_timezone_set('Asia/Manila');
    $date = date("m/d/Y");
    $result=mysqli_query($con, "SELECT SUM(drr_total_head) as total from ddr_table WHERE  drr_date='$date' AND drr_status='1' ");
    $data=mysqli_fetch_assoc($result);
    if($data['total'] =='0'){
    }
    else{
        echo number_format($data['total'],2);
    }
}

