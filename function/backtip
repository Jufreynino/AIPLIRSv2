function fetch_all_mid_postmortem_lung()
{
    global $con;
        $sql    =    "SELECT DISTINCT(pm_table.lung_cause) as lung_cause FROM disease_report_tbl INNER JOIN pm_table ON disease_report_tbl.drr_id = pm_table.pm_id";
        $result = mysqli_query($con, $sql); 
        while($row = mysqli_fetch_assoc($result)){

            $sqls    =    "SELECT SUM(pm_table.lung_number_of_affected) as lung_number_of_affected,SUM(pm_table.lung_weight) as lung_weights, disease_tbl.disease_description as disease_description FROM disease_tbl LEFT JOIN pm_table ON disease_tbl.d_id = pm_table.lung_cause WHERE disease_level='Mid' AND pm_table.lung_cause='".$row['lung_cause']."' GROUP BY disease_tbl.disease_description";
                    $results = mysqli_query($con, $sqls); 
                    while($rows = mysqli_fetch_assoc($results)){

                    ?>
                    <tr>
                        <td ><?php echo $rows['disease_description'] ?></td>
                        <td class="text-right"><?php echo $rows['lung_number_of_affected'] ?></td>
                        <td class="text-right"><?php echo $rows['lung_weights'] ?></td>
                    </tr>
                    <?php
                 
    }
    }
}


