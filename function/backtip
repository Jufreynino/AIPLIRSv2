function add_meat_establishment_account()
{
    if(isset($_POST['btn_add_meat_establishment']))
    {
        global $con;
        date_default_timezone_set('Asia/Manila');
        $today = date("m/d/Y h:i:sa");
        $todays_date = date("m/d/Y");
        $time = date("h:i:sa");



        $code = 'sadsas';
        $me_plant_name = mysqli_real_escape_string($con, $_POST['me_plant_name']);
        $me_category = mysqli_real_escape_string($con, $_POST['me_category']);
        $me_expiration_date = mysqli_real_escape_string($con, $_POST['me_expiration_date']);
        $me_classification = mysqli_real_escape_string($con, $_POST['me_classification']);
        $me_email = mysqli_real_escape_string($con, $_POST['me_email']);
        $me_lto = mysqli_real_escape_string($con, $_POST['me_lto']);
        $me_contact = mysqli_real_escape_string($con, $_POST['me_contact']);
        $region_get = mysqli_real_escape_string($con, $_POST['region_get']);
        $province = mysqli_real_escape_string($con, $_POST['province']);
        $city = mysqli_real_escape_string($con, $_POST['city']);
        $barangay = mysqli_real_escape_string($con, $_POST['barangay']);
        $me_address = mysqli_real_escape_string($con, $_POST['me_address']);
        $me_username = mysqli_real_escape_string($con, $_POST['me_username']);
        $me_password = mysqli_real_escape_string($con, $_POST['me_password']);
        $me_confirm_password = mysqli_real_escape_string($con, $_POST['me_confirm_password']);
        


        if(empty($me_plant_name))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Meat Establishment is required!</span>                                  
                                        </div>
                                        ';
        }
        else if(empty($me_category))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Category is required!</span>                                  
                                        </div>';
        }

        else if(empty($me_classification))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Classification is required!</span>                                  
                                        </div>';
        }
        else if(empty($me_expiration_date))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">LTO Expiration date is required!</span>                                  
                                        </div>
            ';
        }
        else if(empty($me_contact))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Contact Number required!</span>                                  
                                        </div>
            ';
        }

        else if(empty($me_email)) 
        {


                                return $error_classification = "
                                    <div class='alert bg-danger text-white alert-styled-left alert-dismissible'>
                                            <button type='button' class='close' data-dismiss='alert'><span>&times;</span></button>
                                            <span class='font-weight-semibold'>Email is required!</span>                                  
                                        </div>";
        }

        else if(!filter_var($me_email, FILTER_VALIDATE_EMAIL)) 
        {


                                return $error_classification = "
                                    <div class='alert bg-danger text-white alert-styled-left alert-dismissible'>
                                            <button type='button' class='close' data-dismiss='alert'><span>&times;</span></button>
                                            <span class='font-weight-semibold'>Invalid email format</span>                                  
                                        </div>";
        }
        


         else if(empty($me_username))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Username is required!</span>                                  
                                        </div>
            ';
        }


         else if(empty($me_password))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Password is required!</span>                                  
                                        </div>
            ';
        }
        else if(empty($me_confirm_password))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Cofirm password is required!</span>                                  
                                        </div>
            ';
        }

        else if($me_confirm_password != $me_password)
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Password and Cofirm password not match!</span>                                  
                                        </div>
            ';
        }
        
        else if(empty($region_get))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Region is required!</span>                                  
                                        </div>
            ';
        }

         else if(empty($province))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Province is required!</span>                                  
                                        </div>
            ';
        }

         else if(empty($city))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">City is required!</span>                                  
                                        </div>
            ';
        }


         else if(empty($barangay))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Barangay is required!</span>                                  
                                        </div>
            ';
        }
        else if(empty($me_address))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Address is required!</span>                                  
                                        </div>';
        }

        else

        {

            $check_email = "SELECT * FROM meat_establishment_tbl WHERE me_email='$me_email'";
            $check_email_data = mysqli_query($con, $check_email);

            $check_email_result = mysqli_fetch_assoc($check_email_data);
            if($check_email_result > 0)
            {
                    return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Email is already exists!</span>                                  
                                        </div>';
            }
            else
            {

              $check_username = "SELECT * FROM meat_establishment_tbl WHERE me_username='$me_username'";
              $check_username_data = mysqli_query($con, $check_username);
              $check_username_result = mysqli_fetch_assoc($check_username_data);

              if($check_username_result > 0)
              {
                    return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Username is already exists!</span>                                  
                                        </div>';
              }
              else
              {

                $check_meat_establishment_name = "SELECT * FROM meat_establishment_tbl WHERE me_plant_name='$me_plant_name'";
                $check_meat_establishment_name_data = mysqli_query($con, $check_meat_establishment_name);
                $check_meat_establishment_name_result = mysqli_fetch_assoc($check_meat_establishment_name_data);
                if($check_meat_establishment_name_result > 0 )
                {
                            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Meat Establishment  already exists!</span>                                  
                                        </div>';
                }
                else
                {
                    $sql = mysqli_query($con, "INSERT INTO meat_establishment_tbl (me_code,me_plant_name,me_type,me_classification,me_lto_number,me_region_code,me_province_code,me_city_code,me_barangay_code,me_address,me_date_created,me_expiry,me_username,me_password,me_contact,me_email)
                        VALUES('$code','$me_plant_name','$me_category','$me_classification','$me_lto','$region_get','$province','$city','$barangay','$me_address','$todays_date','$me_expiration_date','$me_username','$me_password','$me_contact','$me_email')");

                         $last_id = $con->insert_id;
                        $mail = new PHPMailer();
                        $mail->isSMTP();  
                        $mail->SMTPOptions = array(
                            'ssl' => array(
                                'verify_peer' => false,
                                'verify_peer_name' => false,
                                'allow_self_signed' => true
                            )
                        );                                          // Set mailer to use SMTP
                        $mail->Host       = 'smtp.gmail.com';  // Specify main and backup SMTP servers
                        $mail->SMTPAuth   = true;                                   // Enable SMTP authentication
                        $mail->Username   = 'nmis.devops@gmail.com';                     // SMTP username
                        $mail->Password   = 'vtdojevqxblcsoao';                              //SMTP password
                        $mail->SMTPSecure = 'tls';                                  // Enable TLS encryption, `ssl` also accepted
                        $mail->Port       = 587;  
                        $mail->isHTML(true);
                        $mail->From = 'admin@noreply.com';  // This HAVE TO be your gmail adress
                        $mail->AddEmbeddedImage('../../global_assets/images/download.png', 'Kartka');

                        $mail->FromName = 'AIPLIRS Notification'; // This is the from name in the email, you can put anything you like here

                        $mail->Subject = 'Successfully Registered';

                        $mail->Body = ' <div style=" margin:auto; width:600px;">
                                <div style="border:2px solid #c1c1c1; padding:50px; border-radius:15px;">

                                    <center>

                                    <img src="cid:Kartka">

                                    <h1>Good day!</h1>
                                    </center>

                                    <p style="text-indent: 100px; font-size:15px;">To activate your email,  Please click the button below.<br><br>
                                    <center>
                                     <a href="http://172.16.8.24/AIPLIRSnative/pages/auth/login_activated.php?me_id='.$last_id.'" target="_blank" style="font-size:15px; background-color:#0c69f5; color:white; text-decoration:none; padding:10px; border-radius:5px;">Click Here</a>
                                    </center>
                                    
                                    <br><br><br>
                                    
                                    <b>Department of Agriculture</b> <br>
                                    <b style="font-size:15px;">National Meat Inspection Service</b><br>
                                    Plant Operation Standards and Monitoring Section ( POSMS )
                                </div>
                           </div>';
                        $mail->AddAddress($me_email);
                        $mail->send();
                            return $error_classification = '
                                <div class="alert bg-success text-white alert-styled-left alert-dismissible">
                                    <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                    <span class="font-weight-semibold">Successfully Register</span><a href="http://172.16.8.24/AIPLIRSnative/pages/auth/login.php" class="alert-link"> Click here</a> to login.
                                </div>
                            ';
        
                       
                }
              }
            }
        }





































        
         // <script type="text/javascript"> -->
         //     window.location = "meat_establishment.php"; -->
         // </script> -->
    }
}

function list_of_meat_dealer_inspected()
{
    global $con;
    $md = $_GET['md'];
    $mdate = $_GET['mdate'];
    $plant_name = $_SESSION['plant_name'];
    $sql = "SELECT *  FROM ddr_table INNER JOIN am_table ON ddr_table.drr_date = am_table.am_date WHERE am_table.am_date='$mdate' AND am_table.me_id='$md' GROUP BY ddr_table.drr_meat_dealer ORDER BY drr_date DESC";
    $result = mysqli_query($con, $sql); 

    while($row = mysqli_fetch_assoc($result)){
    }
}

function count_daily_receive_fit_for_hc_heads()
{
    global $con;
    $md = $_GET['md'];
    $mdate = $_GET['mdate'];
    $plant_name = $_SESSION['plant_name'];



    $result = mysqli_query($con, "SELECT SUM(fit_human_consumption.fit_number_of_head) as fit_number_of_head FROM fit_human_consumption INNER JOIN ddr_table on  ddr_table.drr_id = fit_human_consumption.drr_id WHERE fit_human_consumption.fit_date='$mdate' AND ddr_table.drr_status='1' ORDER BY fit_time DESC");
    $data=mysqli_fetch_assoc($result);

    if($data['fit_number_of_head'] =='0'){


    }
    else{
        echo number_format($data['fit_number_of_head'],2);

    }
}

