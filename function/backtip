        $disease_kind_of_meat = mysqli_real_escape_string($con, $_POST['disease_kind_of_meat']);
        $notifiable = mysqli_real_escape_string($con, $_POST['notifiable']);
        $disease_type = 'PDP';
        $disease_category = 'Postmortem';


        $sql = mysqli_query($con, "INSERT INTO disease_tbl (disease_code,disease_description,disease_level,disease_status,disease_type,disease_category,disease_date,disease_time,disease_notifiable,disease_kind_of_meat,disease_species)
                                                                VALUES('$disease_code','$disease_description','Critical','Active','$disease_type','$disease_category','$date','$time','$notifiable','$disease_kind_of_meat','$disease_kind_of_species')");

        ?>
        <script type="text/javascript">
            window.location = "postmortem_pdp.php";
        </script>
        <?php
    }
}

function add_postmortem_slaughter_disease()
{
    if(isset($_POST['btn_add_slaughter_postmortem']))
    {
        global $con;
        date_default_timezone_set('Asia/Manila');
        $today = date("m/d/Y h:i:sa");
        $date = date("m/d/Y");
        $time = date("h:i:sa");
        $disease_code = mysqli_real_escape_string($con, $_POST['disease_code']);
        $disease_description = mysqli_real_escape_string($con, $_POST['disease_description']);
        $disease_level = mysqli_real_escape_string($con, $_POST['disease_level']);
        $disease_kind_of_species = mysqli_real_escape_string($con, $_POST['disease_kind_of_species']);
        $disease_kind_of_meat = mysqli_real_escape_string($con, $_POST['disease_kind_of_meat']);
        $notifiable = mysqli_real_escape_string($con, $_POST['notifiable']);
        $disease_type = 'SLH';
        $disease_category = 'Postmortem';


        // if($disease_kind_of_meat)
        // {
        //     echo "tset";
        // }
        // else
        // {
            $sql = mysqli_query($con, "INSERT INTO disease_tbl (disease_code,disease_description,disease_level,disease_status,disease_type,disease_category,disease_date,disease_time,disease_notifiable,disease_kind_of_meat,disease_species)
            VALUES('$disease_code','$disease_description','Critical','Active','$disease_type','$disease_category','$date','$time','$notifiable','$disease_kind_of_meat','$disease_kind_of_species')");
            // echo "meat";

        // }
        ?>
        <script type="text/javascript">
            window.location = "postmortem_slh.php";
        </script>
        <?php
    }
}

function show_pdp_disease()
{

    global $con;
    $sql = "SELECT * FROM disease_tbl WHERE disease_type='PDP' ORDER BY d_id DESC";
    $result = $con->query($sql);
    while($row = $result->fetch_assoc()) {
        ?>
                <tr style="padding:10px !important; margin:px !important; border:1px solid #bdb8b8;">
                    <td style="font-size:12px !important; "><b> <?php echo $row['disease_code']  ?></b></td>
                    <td style="font-size:12px !important; padding:2px;"><?php echo $row['disease_description']  ?></td>
                    <td style="font-size:12px !important; padding:2px;"><?php echo $row['disease_level']  ?></td>
                   
                    <td style="font-size:12px !important; padding:2px;"><?php echo $row['disease_type']  ?></td>
                    <td style="font-size:12px !important; padding:2px;"><?php echo $row['disease_category']  ?></td>
                    <td style="font-size:12px !important; padding:2px;"><?php echo $row['disease_date'].' '.$row['disease_time']  ?></td>
                    <td style="font-size:12px !important; padding:2px;">
                        <?php if($row['disease_status'] == 'Active') {?>
                         <span class="badge bg-success"><?php echo $row['disease_status']  ?></span>
                        <?php }else{ ?>
                         <span class="badge bg-danger"><?php echo $row['disease_status']  ?></span>
                        <?php } ?>
                    </td>
                    <td style="font-size:12px !important; padding:2px;">


                    <button type="button" class="btn bg-info btn-sm" data-toggle="modal" data-target="#update_pdp<?php echo $row['d_id'];?>"><i class="icon-pencil7"></i></button>


                                <div id="update_pdp<?php echo $row['d_id'];  ?>" class="modal fade" tabindex="-1" data-backdrop="static">
                                    <div class="modal-dialog">
                                        <div class="modal-content">
                                            <div class="modal-header bg-info">
                                                <h6 class="modal-title"><?php echo $row['disease_description']  ?></h6>
                                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                                            </div>
                                            <div class="modal-body">
                                                <form method="POST">
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-sm-4">
                                                                <label>Code</label>
                                                                <input type="text" value="<?php echo $row['disease_code'] ?>" name="disease_code" class="form-control" required>
                                                                <input type="hidden" value="<?php echo $row['d_id'] ?>" name="disease_id" class="form-control" required>

                                                            </div>

                                                            <div class="col-sm-8">
                                                                <label>Disease</label>
                                                                <input type="text" value="<?php echo $row['disease_description'] ?>" name="disease_description" class="form-control" required>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    
                                                    <div class="form-group">
                                                        <div class="row">

                                                        <div class="col-sm-4">
                                                                <label>Level</label>

                                                                <select class="form-control" name="disease_level" required>
                                                                    <option value="<?php echo $row['disease_level'] ?>"><?php echo $row['disease_level'] ?></option>
                                                                    <option value="Low">Low</option>
                                                                    <option value="Mid">Mid</option>
                                                                    <option value="Critical">Critical</option>
                                                                </select>
                                                            </div>
                                                           
                                                            <div class="col-sm-4">
                                                                <label>Category</label>

                                                                <select class="form-control" name="disease_category" required>
                                                                    <option value="<?php echo $row['disease_category'] ?>"><?php echo $row['disease_category'] ?></option>
                                                                    <option value="Antemortem">Antemortem</option>
                                                                    <option value="Postmortem">Postmortem</option>
                                                                </select>
                                                            </div>

                                                            <div class="col-sm-4">
                                                                <label>Status</label>
                                                                <select class="form-control" name="disease_status" required>
                                                                    <option value="<?php echo $row['disease_status'] ?>"><?php echo $row['disease_status'] ?></option>
                                                                    <option value="Active">Active</option>
                                                                    <option value="Inactive">Inactive</option>
                                                                </select>
                                                            </div>

                                                        </div>
                                                    </div>
                                                  
                                             </div>
                                            <div class="modal-footer">
                                                <button type="button" class="btn btn-link" data-dismiss="modal">Close</button>
                                                <button type="submit" class="btn bg-primary" name="btn_update_pdp_disease">Save Changes</button>
                                            </div>
                                        </div>
                                    </form>
                                    </div>
                                </div>
                    </td>
                </tr>
        <?php
        
        if(isset($_POST['btn_update_pdp_disease']))
        {
            $disease_code = mysqli_real_escape_string($con, $_POST['disease_code']);
            $disease_description = mysqli_real_escape_string($con, $_POST['disease_description']);
            $disease_level = mysqli_real_escape_string($con, $_POST['disease_level']);
            $disease_category = mysqli_real_escape_string($con, $_POST['disease_category']);
            $disease_status = mysqli_real_escape_string($con, $_POST['disease_status']);
            $disease_id = mysqli_real_escape_string($con, $_POST['disease_id']);
            

            $sql = mysqli_query($con, "UPDATE disease_tbl SET disease_code='$disease_code', disease_description='$disease_description',
            disease_level='$disease_level',
            disease_category='$disease_category', 
            disease_status='$disease_status' WHERE d_id ='$disease_id'");

                ?>
                <script type="text/javascript">
                    window.location = "poultry.php";
                </script>
                <?php
        }
        
    }



}







function show_pdp_disease_antemortem()
{

    global $con;
    $sql = "SELECT * FROM disease_tbl WHERE disease_type='PDP' AND disease_category='Antemortem' AND disease_delete_status='0' ORDER BY d_id DESC";
    $result = $con->query($sql);
    while($row = $result->fetch_assoc()) {
        ?>
                <tr style="padding:10px !important; margin:px !important; border:1px solid #bdb8b8;">
                    <td style="font-size:12px !important; "><b> <?php echo $row['disease_code']  ?></b></td>
                    <td style="font-size:12px !important; padding:2px;"><?php echo $row['disease_description']  ?></td>
                    <td style="font-size:12px !important; padding:2px;"><?php echo $row['disease_level']  ?></td>
                   
