}

function modify_antemortem_sum_total_weight()
{
    global $con;
    $id     =   $_GET['mid'];
    date_default_timezone_set('Asia/Manila');
    $today = date("m/d/Y h:i:sa");
    $date = date("m/d/Y");
    $time = date("h:i:sa");
    $sum = 0;
    


    $sql = "SELECT am_table.c_weight as c_weight, am_table.r_weight as r_weight, ddr_table.drr_total_weight as drr_total_weight FROM ddr_table INNER JOIN am_table on ddr_table.drr_id = am_table.drr_id  WHERE am_table.drr_id='$id'";

    $result = $con->query($sql);

        if ($result->num_rows > 0) {
            while($row = $result->fetch_assoc()) {

                        $sum +=  floatval($row['c_weight']) +  floatval($row['r_weight']);
                        $total =  $row['drr_total_weight'] - $sum;
                    
                    }
                
                    echo number_format($total,2);
                    ?>
                        <input type="hidden" name="total_of_weight" id="total_number_of_weight_received" value="<?php echo $total?>">
                    <?php
                }
        else{
            $sql = "SELECT * FROM ddr_table  WHERE drr_id='$id' ";
            $result = $con->query($sql);
            $row = $result->fetch_assoc();
            $totalsss =  $row['drr_total_weight'];
            echo number_format($totalsss,2);
            ?>
            <input type="hidden" name="total_of_weight" id="total_number_of_weight_received" value="<?php echo $totalsss?>">
           <?php
        }
       
}




