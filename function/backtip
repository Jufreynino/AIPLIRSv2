    <?php
    if(isset($_POST['btn_update_meat_establishment']))
    {

         $me_plant_name = $_POST['me_plant_name'];
                        $me_category = $_POST['me_category'];
                        $me_lto = $_POST['me_lto'];
                        $me_expiration_date = $_POST['me_expiration_date'];
                        $me_classification = $_POST['me_classification'];
                        $me_region = $_POST['me_region'];
                        $me_province = $_POST['me_province'];
                        $me_city = $_POST['me_city'];
                        $me_barangay = $_POST['me_barangay'];
                        $me_address = $_POST['me_address'];
                        $me_email = $_POST['me_email'];
                        $me_contact = $_POST['me_contact'];
                        $me_username = $_POST['me_username'];
                        $me_password = $_POST['me_password'];
                        $confirm_me_password = $_POST['confirm_me_password'];

        if(empty($me_plant_name))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Meat Establishment is required!</span>                                  
                                        </div>
                                        ';
        }
        else if(empty($me_category))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Category is required!</span>                                  
                                        </div>';
        }

        else if(empty($me_classification))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Classification is required!</span>                                  
                                        </div>';
        }
        else if(empty($me_expiration_date))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">LTO Expiration date is required!</span>                                  
                                        </div>
            ';
        }
        else if(empty($me_contact))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Contact Number required!</span>                                  
                                        </div>
            ';
        }

        else if(empty($me_email)) 
        {


                                return $error_classification = "
                                    <div class='alert bg-danger text-white alert-styled-left alert-dismissible'>
                                            <button type='button' class='close' data-dismiss='alert'><span>&times;</span></button>
                                            <span class='font-weight-semibold'>Email is required!</span>                                  
                                        </div>";
        }

        else if(!filter_var($me_email, FILTER_VALIDATE_EMAIL)) 
        {


                                return $error_classification = "
                                    <div class='alert bg-danger text-white alert-styled-left alert-dismissible'>
                                            <button type='button' class='close' data-dismiss='alert'><span>&times;</span></button>
                                            <span class='font-weight-semibold'>Invalid email format</span>                                  
                                        </div>";
        }
        


         else if(empty($me_username))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Username is required!</span>                                  
                                        </div>
            ';
        }


         else if(empty($me_password))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Password is required!</span>                                  
                                        </div>
            ';
        }
        else if(empty($confirm_me_password))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Cofirm password is required!</span>                                  
                                        </div>
            ';
        }

        else if($confirm_me_password != $me_password)
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Password and Cofirm password not match!</span>                                  
                                        </div>
            ';
        }
        
        else if(empty($me_province))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Region is required!</span>                                  
                                        </div>
            ';
        }

         else if(empty($me_province))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Province is required!</span>                                  
                                        </div>
            ';
        }

         else if(empty($me_city))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">City is required!</span>                                  
                                        </div>
            ';
        }


         else if(empty($me_barangay))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Barangay is required!</span>                                  
                                        </div>
            ';
        }
        else if(empty($me_address))
        {
            return $error_classification = '
                                        <div class="alert bg-danger text-white alert-styled-left alert-dismissible">
                                            <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                            <span class="font-weight-semibold">Address is required!</span>                                  
                                        </div>';
        }

        else

        {

           

             

                


               

                        $sql = mysqli_query($con, "UPDATE meat_establishment_tbl SET

                            me_plant_name ='$me_plant_name',
                            me_type ='$me_category',
                            me_lto_number ='$me_lto',
                            me_expiry ='$me_expiration_date',
                            me_classification ='$me_classification',
                            me_region_code ='$me_region',
                            me_province_code ='$me_province',
                            me_city_code ='$me_city',
                            me_barangay_code ='$me_barangay',
                            me_address ='$me_address',
                            me_email ='$me_email',
                            me_contact ='$me_contact',
                            me_username ='$me_username',
                            me_password ='$me_password',
                            me_status ='Pending'




                        WHERE me_id='".$_SESSION['me_id']."' ");



                            return $error_classification = '

                                <div class="alert bg-success text-white alert-styled-left alert-dismissible">
                                    <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
                                    <span class="font-weight-semibold">Successfully Updated</span>
                                </div>

                            
                            ';




            }





    }
}


function add_mio_account_posms(){
    global $con;
    date_default_timezone_set('Asia/Manila');
    $todays_date = date("m/d/Y h:i:sa");
    $today = strtotime($todays_date);
   
if(isset($_POST['btn_add_mio'])){


