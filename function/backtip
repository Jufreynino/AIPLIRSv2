function mmir_mio()
{

    global $con;
    $md = $_GET['meat_establishment'];

    $from = $_GET['from'];
    $fromdate = DateTime::createFromFormat('Y-m-d',$from);
    $from_date = $fromdate->format('m/d/Y');


    $to = $_GET['to'];
    $todate = DateTime::createFromFormat('Y-m-d',$to);
    $to_date = $todate->format('m/d/Y');


    $plant_name = $_SESSION['plant_name'];


    $sql = "SELECT *  FROM ddr_table  WHERE drr_date BETWEEN '$from_date' AND '$to_date'";
    $result = mysqli_query($con, $sql); 
    if($result->num_rows > 0)
    {

        $sqls = "SELECT *  FROM ddr_table  WHERE drr_date BETWEEN '$from_date' AND '$to_date' AND me_id='$md' ";
        $results = mysqli_query($con, $sqls); 

        ?>
                <div class="row">
                    <div class="col-xl-12">
                        <div class="card card-collapsed" style="background-color:#d7d7d7;">
                            <div class="card-header header-elements-inline">
                                <h4 class="card-title" style="font-weight:bold;"></h4>
                                    <div class="header-elements">
                                        <div class="list-icons">
                                            <a class="list-icons-item" data-action="collapse"></a>
                                        </div>
                                    </div>

                            </div>
                            <div class="card-body py-0">
                                    <h1 class="text-center">I. ANTEMORTEM INSPECTION</h1>
                                <div class="row">

                                    <div class="col-xl-3 col-lg-6" style="padding-left: 5px !important; padding-right: 0;">
                                        <div class="card " style="border:1px solid black;">
                                            <div class="card-statistic-3 p-4">
                                                <div class="card-icon card-icon-large"><i class="fas fa-shopping-cart"></i></div>
                                                <div class="mb-4">
                                                    <h6 class="card-title mb-0">Received Live Animals</h6>
                                                </div>
                                                        <table class="table">
                                                            <tbody>
                                                              <tr>
                                                                    <th style="padding: 4px !important; font-size: 13px !important;">Species of Food Animals</th>
                                                                    <th style="padding: 4px !important; font-size: 13px !important;">Total Number of Animals Received</th>
                                                                    <th style="padding: 4px !important; font-size: 13px !important;">Origin</th>
                                                                </tr>
                                                                    <?php  while($row = mysqli_fetch_assoc($results)){ ?>
                                                                            <tr>
                                                                                <td style="padding: 4px !important; font-size: 13px !important;"><?php echo $row['drr_species'] ?></td>
                                                                                <td style="padding: 4px !important; font-size: 13px !important;"><?php echo $row['drr_total_head'] ?></td>
                                                                                <td style="padding: 4px !important; font-size: 13px !important;"><?php echo $row['drr_farm_origin'] ?></td>
                                                                            </tr>
       
                                                                    <?php  } ?>
                                                            </tbody>
                                                        </table>
                                                </div>
                                        </div>
                                    </div>

                                    <div class="col-xl-6 col-lg-12">
                                        <div class="card " style="border:1px solid black;">
                                            <div class="card-statistic-3 p-4">
                                                <div class="card-icon card-icon-large"><i class="fas fa-shopping-cart"></i></div>
                                                <div class="mb-4">
                                                    <h5 class="card-title mb-0">Conduct Antemortem Inspection</h5>
                                                </div>
                                                       <div class="row">
                                                                <div class="col-md-12">
                                                                    <h6>Rejected For Slaughter</h6>
                                                                    <table class="table"  border='1'>
                                                                        <tbody>
                                                                                <?php 
                                                                                                    
                                                                                
                                                                                $sqlr = "SELECT * FROM am_table INNER JOIN ddr_table ON am_table.drr_id=ddr_table.drr_id WHERE am_table.am_date BETWEEN '$from_date' AND '$to_date' AND am_table.me_id='$md'  ORDER BY am_table.am_time DESC";
                                                                                $resultr = mysqli_query($con, $sqlr); 
                                                                                        ?>

                                                                                            <tr>
                                                                                                <th style="font-size:12px !important; background-color:#324147; color:white;">Species</th>
                                                                                                <th style="font-size:12px !important; background-color:#324147; color:white;">Cause</th>
                                                                                                <th style="font-size:12px !important; background-color:#324147; color:white;">Heads</th>
                                                                                                <th style="font-size:12px !important; background-color:#324147; color:white;">Weight</th>
                                                                                            </tr>
                                                                                        <?php 

                                                                                            
                                                                                while($rowr = mysqli_fetch_assoc($resultr)){
                                                                                
                                                                                ?>
                                                                                   
                                                                                    <tr>
                                                                                        <?php  if($rowr['r_cause'] == '0' || $rowr['r_heads'] == '0' || $rowr['r_weight'] == '0' ){ ?>
                                                                                    
                                                                                            <?php } else{ ?>
                                                                                             
                                                                                                    <td style="font-size:12px !important;"><?php echo $rowr['drr_species'] ?></td>
                                                                                                    <td style="font-size:12px !important;"><?php echo $rowr['r_cause'] ?></td>
                                                                                                    <td style="font-size:12px !important;"><?php echo $rowr['r_heads'] ?></td>
                                                                                                    <td style="font-size:12px !important;"><?php echo $rowr['r_weight'] ?></td>
                                                                                            <?php } ?>
                                                                                                </tr>
                                                                                                  <tr>


                                                                                        <?php  if($rowr['c_cause'] == '0' || $rowr['c_heads'] == '0' || $rowr['c_weight'] == '0' ){ ?>
                                                                                    
                                                                                            <?php } else{ ?>
                                                                                                    <td style="font-size:12px !important;"><?php echo $rowr['drr_species'] ?></td>
                                                                                                    <td style="font-size:12px !important;"><?php echo $rowr['c_cause'] ?></td>
                                                                                                    <td style="font-size:12px !important;"><?php echo $rowr['c_heads'] ?></td>
                                                                                                    <td style="font-size:12px !important;"><?php echo $rowr['c_weight'] ?></td>
                                                                                            <?php } ?>
                                                                                        </tr>
                                                                                <?php } ?>
                                                                            </tbody>
                                                                        </table>
                                                                </div>
                                                       </div>
                                                </div>
                                        </div>
                                    </div>


                                  <div class="col-xl-3 col-lg-6" style="padding-left: 5px !important; padding-right: 0;">
                                        <div class="card " style="border:1px solid black;">
                                            <div class="card-statistic-3 p-4">
                                                <div class="card-icon card-icon-large"><i class="fas fa-shopping-cart"></i></div>
                                                <div class="mb-4">
                                                    <h6 class="card-title mb-0">Received Live Animals</h6>
                                                </div>
                                                        <table class="table">
                                                            <tbody>

                                                              <tr>
                                                                    <th style="padding: 4px !important; font-size: 13px !important;">Species of Food Animals</th>
                                                                    <th style="padding: 4px !important; font-size: 13px !important;">Total Number of Animals Received</th>
                                                                    <th style="padding: 4px !important; font-size: 13px !important;">Origin</th>
                                                                </tr>
                                                                <?php 
                                                                $sqlmmir = "SELECT SUM(fhc_table.fhc_number_of_heads) FROM fhc_table INNER JOIN ddr_table ON fhc_table.drr_id=ddr_table.drr_id WHERE fhc_table.fhc_date BETWEEN '$from_date' AND '$to_date' AND fhc_table.me_id='$md'  ORDER BY fhc_time.fhc_time DESC";
                                                                                $resultmmir = mysqli_query($con, $sqlmmir);  
                                                                                 while($rowmmir = mysqli_fetch_assoc($resultmmir)){ ?>
                                                                            <tr>
                                                                                <td style="padding: 4px !important; font-size: 13px !important;"><?php echo $rowmmir['drr_species'] ?></td>
                                                                                <td style="padding: 4px !important; font-size: 13px !important;"><?php echo $rowmmir['drr_total_head'] ?></td>
                                                                                <td style="padding: 4px !important; font-size: 13px !important;"><?php echo $rowmmir['drr_farm_origin'] ?></td>
                                                                            </tr>
       
                                                                    <?php  } ?>
                                                            </tbody>
                                                        </table>
                                                </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
        <?php
    }
    else
    {
        echo 'red';
    }
}

function dmir_list_mio(){
    global $con;
    $md = $_GET['md'];
    $mdate = $_GET['mdate'];
    $plant_name = $_SESSION['plant_name'];
    $sql = "SELECT *  FROM ddr_table INNER JOIN fhc_table ON ddr_table.drr_id = fhc_table.drr_id INNER JOIN fit_human_consumption ON ddr_table.drr_id=fit_human_consumption.drr_id  WHERE ddr_table.drr_date='$mdate' AND ddr_table.me_id='$md' AND ddr_table.drr_status='1'  ORDER BY ddr_table.drr_time DESC";
    $result = mysqli_query($con, $sql); 

    while($row = mysqli_fetch_assoc($result)){
        ?>
                <div class="col-md-4">
						<div class="card" style="background-color:#ffffff; border-radius:2px; border:1px solid #746b6b;">
						<div class="card-header text-center" style="background-color:#324147; color:white;">
							<h4 class="card-title"><?php echo $row['drr_meat_dealer'] ?><p style="font-size:15px;">Meat Dealer name</p> </h4>

							</div>
							<table style="padding:20px;" class="table-bordered">
								<tr>
									<th>Species </th>
									<td class="text-right"><?php echo $row['drr_species'] ?></td>
								</tr>
								<tr>
									<th colspan="2" class="text-center text-primary">Received live animals</th>
								</tr>
								<tr>
									<th>Head Count  </th>
									<td class="text-right"><?php echo number_format( $row['drr_total_head']);  ?></td>
								</tr>
								<tr>
									<th>Total Weight  </th>
									<td class="text-right"><?php echo number_format( $row['drr_total_weight'],2);  ?></td>
								</tr>
								<tr>
									<th colspan="2" class="text-center text-primary">Fit for Slaughter</th>
								</tr>
								<tr>
									<th>Head Count  </th>
									<td class="text-right"><?php echo number_format( $row['fhc_number_of_heads']);  ?></td>
								</tr>
								<tr>
									<th>Total Weight  </th>
									<td  class="text-right"><?php echo number_format( $row['fhc_weight'],2);  ?></td>
								</tr>
								<tr>
									<th colspan="2" class="text-center text-primary">Fit for Human Consumption</th>
								</tr>
								<tr>
									<th>Head Count  </th>
									<td  class="text-right"><?php echo number_format( $row['fit_number_of_head']);  ?></td>
								</tr>
								<tr>
									<th>Total Weight  </th>
									<td  class="text-right"><?php echo number_format( $row['fit_weight'],2);  ?></td>
								</tr>
								<tr>
									<td  class="text-right" colspan="2">
									<button type="button" class="btn bg-primary" data-toggle="modal" data-target="#view_more_details">View Details </button>
										<button class="btn btn-primary">View Details</button></td>

								</tr>
							</table>
						</div>
					</div>

        <?php
    }
}
function dmir_mio()
{

    global $con;
    $md = $_GET['md'];
    $mdate = $_GET['mdate'];
    $plant_name = $_SESSION['plant_name'];
    $sql = "SELECT *  FROM ddr_table INNER JOIN fhc_table ON ddr_table.drr_id = fhc_table.drr_id WHERE ddr_table.drr_date='$mdate' AND ddr_table.me_id='$md' AND ddr_table.drr_status='1'  ORDER BY ddr_table.drr_time DESC";
    $result = mysqli_query($con, $sql); 

