                                        <td style="width:150px;"><?php echo $row['intestine_weight'] ?></td>
                                       
                                        <td style="width:150px;"><?php echo $row['feet_number_of_affected'] ?></td>
                                        <td style="width:150px;"><?php echo $row['feet_weight'] ?></td>
                                        <td style="width:150px;"><?php echo $row['kidney_number_of_affected'] ?></td>
                                        <td style="width:150px;"><?php echo $row['kidney_weight'] ?></td>
                                        <td style="width:150px;"><?php echo $row['spleen_number_of_affected'] ?></td>
